+/* handles the various endian orders and depths */
+#define ZINDEX32(x, y, img) ((y) * img->bytes_per_line) + ((x) << 2)
+#define ZINDEX16(x, y, img) ((y) * img->bytes_per_line) + ((x) << 1)
+#define ZINDEX8(x, y, img) ((y) * img->bytes_per_line) + (x)
+#define ZINDEX1(x, y, img) ((y) * img->bytes_per_line) + ((x) >> 3)+static unsigned long byteorderpixel = MSBFirst << 24;
+void PutPixel(XImage* ximage, int x, int y, unsigned long pixel)
+{
+  unsigned char *addr;+  if (ximage->bits_per_pixel == 32) 
+  {+     addr = &((unsigned char *)ximage->data) [ZINDEX32(x, y, ximage)];
+     if(*((char *)&byteorderpixel) == ximage->byte_order)
+     {
+       *((unsigned long *)addr) = pixel;
+     } else if(ximage->bitmap_bit_order == MSBFirst)
+     {
+       addr[0] = pixel >> 24;+       addr[1] = pixel >> 16;+       addr[2] = pixel >> 8;
+       addr[3] = pixel;+     } else +     {+       addr[3] = pixel >> 24; +       addr[2] = pixel >> 16;+       addr[1] = pixel >> 8;+       addr[0] = pixel;
+     }
+  } else if(ximage->bits_per_pixel == 16)
+  {
+     addr = &((unsigned char *)ximage->data) [ZINDEX16(x, y, ximage)];
+     if(ximage->bitmap_bit_order == MSBFirst)
+     {
+       addr[0] = pixel >> 8;+       addr[1] = pixel;
+     } else 
+     {
+       addr[1] = pixel >> 8;+       addr[0] = pixel;
+     }+  } else if(ximage->bits_per_pixel == 8)
+  {
+     ximage->data[ZINDEX8(x, y, ximage)] = pixel;
+     return;
+  } else if (ximage->bits_per_pixel == 1)
+  {
+     if(ximage->bitmap_bit_order == MSBFirst)
+     {
+       if (pixel & 1)
+         ximage->data[ZINDEX1(x, y, ximage)] |= 0x80 >> (x & 7);
+       else
+         ximage->data[ZINDEX1(x, y, ximage)] &= ~(0x80 >> (x & 7));
+      } else
+      {
+        if (pixel & 1)
+          ximage->data[ZINDEX1(x, y, ximage)] |= 1 << (x & 7);
+        else
+          ximage->data[ZINDEX1(x, y, ximage)] &= ~(1 << (x & 7));
+      }
+  }
+}
