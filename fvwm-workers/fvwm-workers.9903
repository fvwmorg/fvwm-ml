int main (int argc, char **argv)

  {
  char *s;
  char *displayName = NULL;

  /* Save the program name for error messages and config parsing */
  s = strrchr (argv [0], '/');
  Module = s? s + 1: argv [0];
  if ((argc != 6) && (argc != 7))
    {
    fprintf (stderr, "%s Version %s should only be executed by fvwm!\n",
     Module, VERSION);
    exit(1);
    }  
  Fvwm_fd [0] = atoi (argv [1]);
  Fvwm_fd [1] = atoi (argv [2]);
  signal (SIGPIPE, DeadPipe);
  #ifdef LOGFILE
    log = fopen (LOGFILE, "a");
    fprintf (log, "Starting FvwmStreets\n");
    #endif
  cur_channel = 0;
  ParseConfig ();
  fd_width = GetFdWidth ();
  SetMessageMask (Fvwm_fd, M_STRING);
  EndLessLoop ();
  }
