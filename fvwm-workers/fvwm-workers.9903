------------------------------------------------------------------------

LD_RUN_PATH=/usr/X11R6.1/lib gcc -s -O2 -msupersparc -I/usr/X11R6.1/include -L/u/eddy/lib -L/usr/X11R6.1/lib -o xgetprisel xgetprisel.c -lX11 -lsocket -lnsl

#include <stdio.h>
#include <stdlib.h>
#include <errno.h>
#include <sys/types.h>
#include <X11/X.h>
#include <X11/Xlib.h>
#include <X11/Xatom.h>
#include <X11/Xproto.h>
#include <X11/Xresource.h>

#define kDefaultPropertySize (10240L)

static Display *gDisplay;

static char *MyAlloc(char *str, int size)
{
    char *ret;

    if (size == 0)
	size = 1;

    if (str == (char *)0) {
	ret = malloc((unsigned)size);
    } else {
	ret = realloc(str, (unsigned)size);
    }

    if (ret == (char *)0) {
	XBell(gDisplay, 0);
	XFlush(gDisplay);
	exit(2);
    }
    return ret;
}

/* Request and retrieve a copy of the current selection property */
unsigned char *GetSelection(Display *dpy, Window window, Atom atom)
{
    XEvent event;
    struct timeval timeout = {3, 0};
    fd_set serverFDMask;
    Atom actualType;
    int actualFormat;
    unsigned long numItems, bytesAfter;
    unsigned char *data;
    unsigned char *alreadyGot;
    unsigned long size;

    /* tell the server we want a copy of the selection */
    XConvertSelection(dpy, atom, XA_STRING, XA_STRING, window, CurrentTime);

    FD_ZERO(&serverFDMask);
    FD_SET(ConnectionNumber(dpy), &serverFDMask);

    do {
	int result;

	do {
	    if (XPending(dpy) > 0) {
		result = 1;
	    } else {
		result = select(FD_SETSIZE, &serverFDMask, (fd_set *)0, (fd_set *)0, &timeout);
		if (result == -1 && errno != EINTR) {
		    XBell(dpy, 0);
		    XFlush(dpy);
		    exit(1);
		}
	    }
	} while (result == -1);

	if (result == 0) {
	    return (unsigned char *)0;
	}

	XNextEvent(dpy, &event);
    } while (event.type != SelectionNotify);

    if (event.xselection.property == None) {
	/* Retrieving the selection failed for some reason */
	return (unsigned char *)0;
    }

    if (XGetWindowProperty(dpy, event.xselection.requestor, event.xselection.property, 0L, kDefaultPropertySize, True, AnyPropertyType, &actualType, &actualFormat, &numItems, &bytesAfter, (unsigned char **)&data) != Success) {
	/* The get failed for some reason */
	return (unsigned char *)0;
    }

    if (bytesAfter > 0) {
	alreadyGot = (unsigned char *)MyAlloc(data, numItems + bytesAfter + 1);
	size = numItems;
    } else {
	return data;
    }

    while (bytesAfter > 0) {
	if (XGetWindowProperty(dpy, event.xselection.requestor, event.xselection.property, size / 4, kDefaultPropertySize, True, AnyPropertyType, &actualType, &actualFormat, &numItems, &bytesAfter, (unsigned char **)&data) != Success) {
	    /* The get failed for some reason */
	    return alreadyGot;
	} else {
	    (void)memcpy(alreadyGot + size, data, numItems);
	    size += numItems;
	    XFree(data);
	}
    }

    return alreadyGot;
}

/* Print the primary selection to standard out and return 0. If the
   primary selection is invalid, print CUT_BUFFER0 to standard out and
   return 0. If any errors occur (no selection in either place, for 
   example), return 1. */
int main(int argc, char *argv[])
{
    Window window;
    Atom selectionAtom = (Atom)0;
    unsigned char *currentSelection = (unsigned char *)0;
    int result;
    int	bytes;

    if ((gDisplay = XOpenDisplay((char *)0)) == (Display *)0) {
	fprintf(stderr, "Couldn't open display %s\n", XDisplayName((char *)0));
	exit(1);
    }

    if ((selectionAtom = XInternAtom(gDisplay, "PRIMARY", 1)) != (Atom)0) {
	/* This window never gets mapped - its simply used as an ID */
	window = XCreateSimpleWindow(gDisplay, DefaultRootWindow(gDisplay), 0, 0, 1, 1, 0, 0, 0);

	/* Get the selection */
	if ((currentSelection = GetSelection(gDisplay, window, selectionAtom)) == (unsigned char *)0) {
	    XConvertSelection(gDisplay, XA_PRIMARY, XA_STRING, XA_CUT_BUFFER0,
			XDefaultRootWindow(gDisplay), CurrentTime);
	    currentSelection = XFetchBytes(gDisplay, &bytes);
	}
    }
    
    if (currentSelection == (unsigned char *)0 || *currentSelection == '\0') {
	XBell(gDisplay, 0);
	XFlush(gDisplay);
	result = 1;
    } else {
	/* Print out the selection *without* appending a new line */
	printf("%s", currentSelection);
	result = 0;
    }

    XCloseDisplay(gDisplay);

    return result;
}
