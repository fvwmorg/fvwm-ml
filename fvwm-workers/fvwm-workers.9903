-MenuRoot *FindPopup(char *action)
+MenuRoot *FindPopupGetTok(char *action)
 {
   char *tmp;
   MenuRoot *mr;
@@ -461,6 +461,24 @@
   free(tmp);
   return NULL;
 
+}
+
+MenuRoot *FindPopup(char *action)
+{
+  MenuRoot *mr;
+
+  if(action == NULL)
+    return NULL;
+
+  mr = Scr.menus.all;
+  while(mr != NULL)
+  {
+    if(mr->name != NULL)
+      if(strcasecmp(action,mr->name)== 0)
+        return mr;
+    mr = mr->next;
+  }
+  return NULL;
 }
 
 
