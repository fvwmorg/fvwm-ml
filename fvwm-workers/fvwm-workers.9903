    printf ("GotoNearestLotV: \"%s\", %d, %d, %d\n", args, old.d, old.x, old.y);
    #endif
  /* Traverse to find the closest lot on this desk */
  for (temp = grid;; temp = temp->next)
    {
    if (temp == NULL)
      {
      if (least_distance < MAXINT)
        {
        sprintf (command, "Scroll %dp %dp", new.x - old.x, new.y - old.y);
        SendFvwmPipe (command, 0);
        }
      else
        {
        /* No lots on this desk; do nothing */
        }
      break;
      }
    if (old.d > temp->d_min && old.d < temp->d_max
     && old.c > temp->c_min && old.c < temp->c_max
     && (old.d - temp->d) % temp->d_cycle == 0
     && (old.c - temp->c) % temp->c_cycle == 0)
      {
      temp_distance = (temp->x - old.x) * (temp->x - old.x)
                    + (temp->y - old.y) * (temp->y - old.y);
      if (temp_distance < least_distance)
        {
        new.x = temp->x;
        new.y = temp->y;
        least_distance = temp_distance;
        }
      }
    }
  return 0;
  }


int WhereIWasOnDeskV (char *args)

  {
  VillageGrid *temp;
  VillageGrid new;  
  VillageGrid old;
  char *word;
  static VillageGrid *switchaway = NULL;

  /* TODO: replace this crap with a scanf */
  word = next_word (args); /* ignore title */
  new.d = atoi (word); word = next_word (word);
  old.d = atoi (word); word = next_word (word);
  old.x = atoi (word); word = next_word (word);
  old.y = atoi (word);
  #if 0
    printf ("WhereIWasOnDeskV: \"%s\", %d, %d, %d, %d\n",
     args, new.d, old.d, old.x, old.y);
    #endif
  sprintf (command, "Desk 0 %d", new.d);
  SendFvwmPipe (command, 0);
  
  /* Traverse the list to see if we've been on the new desk before */
  for (temp = switchaway;; temp = temp->next)
    {
    if (temp == NULL)
      {
      /* We've never been here, so don't move the viewport */
      new.x = old.x;
      new.y = old.y;
      break;
      }
    if (temp->d == new.d)
      {
      /* We've been here before; return to last position on this desk */
      new.x = temp->x;
      new.y = temp->y;
      sprintf (command, "Scroll %dp %dp", new.x - old.x, new.y - old.y);
      SendFvwmPipe (command, 0);
      break;
      }
    }

  /* Traverse to see if there's already a record for the old desk */
  for (temp = switchaway;; temp = temp->next)
    {  
    if (temp == NULL)
      {   
      /* We never switched away from that desk before, so make a new record */
      temp = malloc (sizeof (VillageGrid));
      temp->x = old.x;
      temp->y = old.y;
      temp->d = old.d;
      temp->next = switchaway;
      switchaway = temp;
      break;
      }
    if (temp->d == old.d)
      {
      /* Use existing record */
      temp->x = old.x;
      temp->y = old.y;
      break;
      }
    }
  return 0;
  }


