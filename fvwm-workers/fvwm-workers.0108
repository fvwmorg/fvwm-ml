  Pete> "Quit" ^C *MyFvwmTalkCommand Nop

  Pete> #######################################################################
  Pete> # FvwmIconMan
  Pete> #######################################################################
  Pete> # this FvwmIconMan setup is for swallowing in above button bar
  Pete> example *FvwmIconMan*numManagers 1 *FvwmIconMan*Resolution page
  Pete> *FvwmIconMan*background bisque3 *FvwmIconMan*foreground black
  Pete> *FvwmIconMan*font 7x13bold *FvwmIconMan*format "%t" *FvwmIconMan*action
  Pete> Mouse 1 N sendcommand Focus, sendcommand "Iconify" *FvwmIconMan*action
  Pete> Mouse 2 N sendcommand "Iconify 1" *FvwmIconMan*action Mouse
  Pete> 3 N sendcommand "FvwmIdent" *FvwmIconMan*followfocus true
  Pete> *FvwmIconMan*sort false *FvwmIconMan*plainbutton up black bisque3
  Pete> *FvwmIconMan*selectbutton down black bisque3 *FvwmIconMan*focusbutton
  Pete> up yellow firebrick *FvwmIconMan*focusandselectButton down yellow
  Pete> firebrick *FvwmIconMan*dontshow icon=Untitled name=Untitled
  Pete> #*FvwmIconMan*drawicons true *FvwmIconMan*buttongeometry 300x0
  Pete> *FvwmIconMan*managergeometry 1x5

  Pete> #######################################################################
  Pete> # FvwmIdent
  Pete> #######################################################################

  Pete> # window identifier # FvwmIdent *FvwmIdentFore
  Pete> yellow *FvwmIdentBack MidnightBlue *FvwmIdentFont
  Pete> -adobe-helvetica-medium-r-*-*-12-*-*-*-*-*-*-*

  Pete> #######################################################################
  Pete> # FvwmPager
  Pete> #######################################################################

  Pete> # Modify this according to your monitor size
  Pete> *FvwmPager: DeskTopScale 25 #*FvwmPager: SloppyFocus *FvwmPagerBack
  Pete> rgb:90/80/90 *FvwmPagerFore rgb:48/40/48 *FvwmPagerFont
  Pete> -adobe-helvetica-bold-r-*-*-10-*-*-*-*-*-*-* # turn off desktop
  Pete> names for swallowing in above button bar example:  #*FvwmPagerFont
  Pete> none *FvwmPagerHilight rgb:CA/B3/CA *FvwmPagerGeometry -3-3 #
  Pete> horizontal, vertical #*FvwmPagerLabel 0 Main #*FvwmPagerLabel
  Pete> 1 Network #*FvwmPagerLabel 2 Emacs #*FvwmPagerLabel 3 Matlab
  Pete> *FvwmPagerSmallFont 5x8 #*FvwmPagerBalloons All #*FvwmPagerBalloonBack
  Pete> Yellow #*FvwmPagerBalloonFore Black #*FvwmPagerBalloonFont
  Pete> lucidasanstypewriter-12 #*FvwmPagerBalloonYOffset +2
  Pete> #*FvwmPagerBalloonBorderWidth 1 #*FvwmPagerBalloonBorderColor Black

  Pete> #######################################################################
  Pete> # FvwmWinList
  Pete> #######################################################################

  Pete> *FvwmWinListBack rgb:90/80/90 FvwmWinListFore black FvwmWinListFont
  Pete> *-adobe-helvetica-bold-r-*-*-10-*-*-*-*-*-*-* FvwmWinListAction Click1
  Pete> *Iconify -1,Focus FvwmWinListAction Click2 Iconify FvwmWinListAction
  Pete> *Click3 Module "FvwmIdent" FvwmIdent FvwmWinListUseSkipList
  Pete> *FvwmWinListGeometry +0-1 FvwmBackerDesk 0 -solid steelblue
  Pete> *FvwmBackerDesk 1 -solid midnightblue

  Pete> #######################################################################
  Pete> #######################################################################
  Pete> # FvwmIconBox - I don't use this so its not properly setup for our
  Pete> config
  Pete> #######################################################################
  Pete> #######################################################################

  Pete> # NOTE: If you ever use this feature, you must set `Style "*" NoIcon'
  Pete> #Style "*" NoIcon

  Pete> *FvwmIconBoxFore black FvwmIconBoxBack black FvwmIconBoxIconFore black
  Pete> *FvwmIconBoxIconBack bisque3 FvwmIconBoxIconHiFore white

  Pete> *FvwmIconBoxIconHiBack slateblue2 *FvwmIconBoxFont
  Pete> -adobe-helvetica-medium-r-*-*-12-*-*-*-*-*-*-* *FvwmIconBoxSortIcons
  Pete> IconName *FvwmIconBoxPadding 3 *FvwmIconBoxPlacement Top
  Pete> Left *FvwmIconBoxLines 10 #*FvwmIconBoxMaxIconSize 64x38
  Pete> *FvwmIconBoxMaxIconSize 54x30 *FvwmIconBoxHilightFocusWin *FvwmIconBox
  Pete> Fvwm* - #*FvwmIconBoxPixmap fvwm.xpm *FvwmIconBoxGeometry 2x5-10+140
  Pete> *FvwmIconBoxSBWidth 3 *FvwmIconBoxFrameWidth 1 1 *FvwmIconBoxHideSC
  Pete> Horizontal *FvwmIconBoxHideSC Vertical *FvwmIconBoxSetWMIconSize
  Pete> *FvwmIconBox: NoIconAction SendToModule FvwmAnimate animate #key
  Pete> bindings *FvwmIconBoxKey r RaiseLower *FvwmIconBoxKey space Iconify
  Pete> *FvwmIconBoxKey d Close *FvwmIconBoxKey n Next *FvwmIconBoxKey p Prev
  Pete> *FvwmIconBoxKey h Left *FvwmIconBoxKey j Down *FvwmIconBoxKey k Up
  Pete> *FvwmIconBoxKey l Right # mouse bindings *FvwmIconBoxMouse 1 Click
  Pete> RaiseLower *FvwmIconBoxMouse 1 DoubleClick Iconify *FvwmIconBoxMouse
  Pete> 2 Click Iconify -1,Focus *FvwmIconBoxMouse 3 Click Module FvwmIdent
  Pete> # icon files *FvwmIconBox "*" unknown1.xpm *FvwmIconBox "Fvwm*"
  Pete> - *FvwmIconBox "rxvt" term.xpm *FvwmIconBox "xterm" xterm.xpm
  Pete> *FvwmIconBox "editres" editres.xpm *FvwmIconBox "xman" xman.xpm
  Pete> *FvwmIconBox "xbiff" mail1.xpm *FvwmIconBox "xmh" mail1.xpm
  Pete> *FvwmIconBox "xcalc" xcalc.xpm *FvwmIconBox "emacs*" gnu-animal.xpm
  Pete> *FvwmIconBox "FvwmButtons" toolbox.xpm

  Pete> #######################################################################
  Pete> # FvwmScroll
  Pete> #######################################################################

  Pete> *FvwmScrollFore green FvwmScrollBack grey40

  Pete> #######################################################################
  Pete> # FvwmAnimate
  Pete> #######################################################################

  Pete> # this sets the default animation *FvwmAnimateEffect TwistRandom
  Pete> # how many milliseconds to sleep between frames of animation.
  Pete> *FvwmAnimateDelay 20 # how many steps to break the animation into
  Pete> #*FvwmAnimateIterations 5 # how many revolutions to twist the
  Pete> iconification frame *FvwmAnimateTwist 5 # how wide a line to draw with.
  Pete> *FvwmAnimateWidth 1

  Pete> #======================================================================
  Pete> =======================================================================
  Pete> # Functions
  Pete> #======================================================================
  Pete> =======================================================================

  Pete> # Fvwm initial startup functions AddToFunc InitFunction

  Pete> # Set deskop backround color, NOTE: this gets overridden when using
  Pete> GNOME + "I" Exec exec xsetroot -solid black #+ "I" Exec exec xsetroot
  Pete> -solid wheat4

  Pete> # Fvwm stuff + "I" Module FvwmPager 0 1 + "I" Desk 0 0 + "I" Module
  Pete> FvwmAnimate #+ "I" Module FvwmAuto 400 raise lower

  Pete> # Xbiff, xclock and xload + "I" Exec exec xclock -update 1 -geometry
  Pete> =220x35-10+5 -bg black - fg green -bd green -hd slateblue -hl red
  Pete> -digital -padding 8 & + "I" Exec exec xclock -update 1 -geometry
  Pete> =105x80-125+50 -bg black - fg green -bd green -hd slateblue -hl red &
  Pete> + "I" Exec exec xload -bg black -fg green -geometry =105x80-10+50 - hl
  Pete> red &

  Pete> # Xterms + "I" Exec exec xterm -ls -xrm "*Page:0 0 0" -geometry
  Pete> 100x45+10+15 & + "I" Exec exec xterm -ls -xrm "*Page:0 1 0" -geometry
  Pete> 90x25+10+15 & + "I" Exec exec xterm -ls -xrm "*Page:0 1 0" -geometry
  Pete> 90x22+10-15 &

  Pete> # Fvwm restart functions AddToFunc RestartFunction man + "I" Module
  Pete> FvwmPager 0 1 + "I" Module FvwmAnimate #+ "I" Module FvwmIconBox

  Pete> # Complex (user) Functions # Auto-raise, resize on motion AddToFunc
  Pete> Resize-Raise-Func "I" Raise + "M" Resize # Auto-lower, resize on motion
  Pete> AddToFunc Resize-Lower-Func "I" Lower + "M" Resize # Auto-raise, move
  Pete> on motion, iconify on double-click AddToFunc Move-or-Iconify-Raise-Func
  Pete> "I" Raise + "M" Move + "D" Iconify # Auto-Lower, move on motion,
  Pete> iconify on double-click AddToFunc Move-or-Iconify-Lower-Func "I" Lower
  Pete> + "M" Move + "D" Iconify # Move on motion, iconify on double-click
  Pete> AddToFunc Move-or-Iconify "M" Move + "D" Iconify # Auto-raise then
  Pete> print AddToFunc PrintFunction "I" Raise + "I" Exec exec xdpr -id $w

  Pete> ~Pete

