lorinder$ cat findlargest 
#!/usr/bin/awk -f

BEGIN {
        if(keep==0)
                keep=10;
        smallestkeptsz=0;
        for(i=0; i<keep; i++)
                A["--FILLER"i"--"]=0;
}

{
        if($5 > smallestkeptsz) {
                for(k in A) {
                        if(A[k]==smallestkeptsz) {
                                delete A[k];
                                A[$9]=$5;
                                smallestkeptsz=$5;
                                break;
                        }
                }

                for(k in A) {
                        if(A[k]<smallestkeptsz) 
                                smallestkeptsz=A[k];
                }
        }
}

END {
        for(k in A)
                print A[k] ":  " k
}
