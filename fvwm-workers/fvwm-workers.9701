Please add following patch to working source code before 2.0.45 comes
out, for portability fix of FvwmIconMan. It uses sysconf().

--- modules/FvwmIconMan/FvwmIconMan.c.dist	Thu Oct 17 03:12:09 1996
+++ modules/FvwmIconMan/FvwmIconMan.c	Thu Jan 16 21:00:32 1997
@@ -6,6 +6,7 @@
 #else
 #include "../../fvwm/module.h"
 #endif
+#include "../../libs/fvwmlib.h"
 
 static int fd_width;
 static FILE *console = NULL;
@@ -211,7 +212,7 @@
   read_in_resources (argv[3]);
   
   assert (globals.managers);
-  fd_width = sysconf(_SC_OPEN_MAX);
+  fd_width = GetFdWidth();
 
   SetMessageMask(Fvwm_fd,M_CONFIGURE_WINDOW | M_RES_CLASS | M_RES_NAME |
                  M_ADD_WINDOW | M_DESTROY_WINDOW | M_ICON_NAME |

-- -
Makoto `MAR_kun' MATSUSHITA
