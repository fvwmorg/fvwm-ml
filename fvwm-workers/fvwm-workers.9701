 {
-    if (!s)
-	return;
+    if (!s) return;
+    while (*s&&isspace(*s))++s;
+    if (!*s) return;
     cur_decor = fl;
     ExecuteFunction(s,NULL,&Event,C_ROOT,-1);
     cur_decor = NULL;
@@ -2499,14 +2500,13 @@
     
     if (found && (found != &Scr.DefaultDecor)) {
 	FvwmWindow *fw = Scr.FvwmRoot.next;
-	
