diff -u -r1.1 fvwm/events.c
--- 1.1 1997/01/08 16:20:25
+++ fvwm/events.c       1997/01/08 17:26:04
@@ -978,6 +978,25 @@
       
     }
   }
+#ifdef CLICKTORAISE
+  else if ((Tmp_win) && !(Tmp_win->flags & ClickToFocus) &&
+          Event.xbutton.window == Tmp_win->frame)
+    {
+      if (Tmp_win != Scr.LastWindowRaised &&
+         (Event.xbutton.state &
+          (ControlMask|Mod1Mask|Mod2Mask|Mod3Mask|Mod4Mask|Mod5Mask)) == 0 &&
+         GetContext(Tmp_win,&Event, &PressedW) == C_WINDOW)
+           {
+             RaiseWindow(Tmp_win);
+             KeepOnTop();
+           }
+      XSync(dpy,0);
+      XAllowEvents(dpy,ReplayPointer,CurrentTime);
+      XSync(dpy,0);
+      return;
+    }
+#endif
+ 
   XSync(dpy,0);
   XAllowEvents(dpy,ReplayPointer,CurrentTime);
   XSync(dpy,0);



