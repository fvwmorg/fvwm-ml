#include <X11/Intrinsic.h>
#include <X11/Shell.h>
#include <X11/StringDefs.h>

int main(int argc, char **argv) 
{   
  XtAppContext appContext;
  Display *display;
  Widget shell;
  Arg al[2];
  short ac = 0;

  XtToolkitInitialize();
  appContext = XtCreateApplicationContext();
  display = XtOpenDisplay(appContext, 0, argv[0], argv[0], 0, 0, &argc,
                          argv);
  XtSetArg(al[ac], XtNheight, 500); ac++;
  XtSetArg(al[ac], XtNwidth, 500); ac++;
  shell = XtAppCreateShell(0, "shell", applicationShellWidgetClass,
                           display, al, ac);

  { /* CKH - don't want window to get focus... */
    XWMHints hints;
    Atom at;

    hints.flags = InputHint;
    hints.input = False;

    if (!XtIsRealized(shell))
    {
      XtRealizeWidget(shell);
    }

    XSetWMHints(display, XtWindow(shell), &hints);
    at = XInternAtom(display,"WM_TAKE_FOCUS",False);
    XDeleteProperty(display,XtWindow(shell),at);
  }

  XtPopup(shell, XtGrabNone);
  XtAppMainLoop(appContext);

  return 0;
}   

