+        }
+        if (yt + fw->frame_g.height > screen_g.y + screen_g.height)
+        {
+          yt = screen_g.y + screen_g.height - fw->frame_g.height;
+        }
+        if (xl < screen_g.x)
+        {
+          xl = screen_g.x;
+        }
+        if (yt < screen_g.y)
+        {
+          yt = screen_g.y;
+        }
+        attr_g->x = xl;
+        attr_g->y = yt;
+      }
