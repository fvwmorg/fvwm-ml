+    case PLACE_UNDERMOUSE:
+      {
+        int mx;
+        int my;
+        FQueryPointer(dpy, Scr.Root, &JunkRoot, &JunkChild,
+          &mx, &my, &JunkX, &JunkY, &JunkMask);
+        xl = mx - (fw->frame_g.width / 2);
+        yt = my - (fw->frame_g.height / 2);
+        if (xl + fw->frame_g.width > screen_g.x + screen_g.width)
+        {
