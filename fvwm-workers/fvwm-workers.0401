diff -ur fvwm-2.5.8-orig/fvwm/events.c fvwm-2.5.8-mod/fvwm/events.c
--- fvwm-2.5.8-orig/fvwm/events.c	2003-08-28 13:26:14.000000000 -0700
+++ fvwm-2.5.8-mod/fvwm/events.c	2004-01-26 17:15:04.000000000 -0800
@@ -1388,6 +1388,14 @@
 		PressedW = None;
 		return False;
 	}
+	/* IgnoreMouseBindings code */
+	if (DO_IGNORE_MOUSE_BINDINGS(exc->w.fw) && exc->w.wcontext == C_WINDOW)
