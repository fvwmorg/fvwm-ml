+    modifiers = hold_mods | new_mods;
+
+    if (e) {
+	keysym = XLookupKeysym(&(e->xkey), 0);
+		    
+	switch (keysym) {
+	  case XK_Shift_L:
+	  case XK_Shift_R:
+	    /* Shift key */
+	    if (e->type == KeyPress)
+		modifiers |= ShiftMask;
+	    else
+		modifiers &= ~ShiftMask;
+	    break;
+	  case XK_Control_L:
+	  case XK_Control_R:
+	    /* Control key */
+	    if (e->type == KeyPress)
+		modifiers |= ControlMask;
+	    else
+		modifiers &= ~ControlMask;
+	    break;
+	  case XK_Meta_L:
+	  case XK_Meta_R:
+	    /* Meta key */
+	    if (e->type == KeyPress)
+		modifiers |= Mod1Mask;
+	    else
+		modifiers &= ~Mod1Mask;
+	    break;
+	  case XK_Alt_L:
+	  case XK_Alt_R:
+	    /* Alt key */
+	    if (e->type == KeyPress)
+		modifiers |= Mod2Mask;
+	    else
+		modifiers &= ~Mod2Mask;
+	    break;
+	  default:
+	    /* No modifier we know to track */
+	    break;
+	}
+    }
+
+    fprintf(stderr, "Modifiers active: ");
+    if (modifiers & ShiftMask)
+	fprintf(stderr, "+Shift");
+    if (modifiers & ControlMask)
+	fprintf(stderr, "+Control");
+    if (modifiers & Mod1Mask)
+	fprintf(stderr, "+Meta(Mod1)");
+    if (modifiers & Mod2Mask)
+	fprintf(stderr, "+Alt(Mod2)");
+    fprintf(stderr, " (0x%02x)\n", modifiers);
+
+    
+    for (mi = MR_FIRST_ITEM(mr) ; mi ; mi = MI_NEXT_ITEM(mi)) {
+	char *action;
+	char *switchcmd;
+	char *postfix;
+
+	if (!MI_IS_MOD_SWITCH(mi))
+	    continue;
+
+	fprintf(stderr, "Found a MOD_SWITCH MenuItem (0x%p): %s\n",
+		mi, mi->ms_action);
+	action = mi->ms_action;
+	/* Process the ModSwitch to see if we have a matching */
+	/* modifier specification... */
+	action = SkipNTokens(action, 1); /* Skip 'ModSwitch' */
+	while ((action = GetNextToken(action, &modspec)) != NULL) {
+	    if (!modspec) {
+		/* Are we done?  Exit quietly with no match?  */
+		break;
+	    }
+
+	    //fprintf(stderr, "action is '%s'\n", action);
+	    if (modifiers_string_to_modmask(modspec, &modmask)) {
+		fvwm_msg(WARN, "__mloop_handle_event",
+			 "Illegal modifier string '%s'", modspec);
+		continue;
+	    }
+	    fprintf(stderr, "Got modmask 0x%x in mi (modspec '%s')\n", modmask, modspec);
+
+	    // Get the parenthesized expression that
+	    // that matches the pressed modifier(s)
+	    action = GetQuotedString(action, &switchcmd, " \t\r\n\0", NULL, "(", ")");
+	    if (modmask != modifiers) {
+		// Try the next one...
+		free(switchcmd);
+		continue;
+	    }
+
+	    // XXX - Will the first char at *switchcmd
+	    // ever *not* be '(' ?
+	    if (*switchcmd != '(') {
+		fvwm_msg(WARN, "__mloop_handle_event",
+			 "Illegal ModSwitch value '%s'", switchcmd);
+		free(switchcmd);
+		continue;
+	    } else {
+		// Prune the ()'s
+		switchcmd = SkipSpaces(switchcmd, "(", 1);
+		postfix = strrchr(switchcmd, ')');
+		if (postfix)
+		    *postfix = '\0';
+	    }
+	    //fprintf(stderr, "\nswitchcmd is '%s', action is '%s'\n", switchcmd, action);
+
+	    switchcmd = GetNextToken(switchcmd, &postfix);
+	    if (!postfix)
+		postfix = "";
+
+	    //fprintf(stderr, "postfix is '%s', switchcmd is now '%s'\n", postfix, switchcmd);
+
+	    // Change the label, and the action
+	    // XXX - should these all be MI_LABEL()[0] ?!?
+	    if (strcmp(MI_LABEL(mi)[0], mi->ms_labelbase))
+		free(MI_LABEL(mi)[0]);
+	    MI_LABEL(mi)[0] = safemalloc(strlen(mi->ms_labelbase) +
+					 strlen(postfix) + 1);
+	    strcpy(MI_LABEL(mi)[0], mi->ms_labelbase);
+	    strcat(MI_LABEL(mi)[0], postfix);
+	    if (strcmp(MI_ACTION(mi), mi->ms_action))
+		free(MI_ACTION(mi));
+	    MI_ACTION(mi) = strdup(switchcmd);
+	    MR_IS_UPDATED(mr) = 1;
+	    fprintf(stderr, "Updated ModSwitch in menu\n");
+	    get_menu_paint_item_parameters(&mpip, mr, mi, NULL, NULL, False);
+	    menuitem_paint(mi, &mpip);
+	    // We're done this menuitem.  Skip to the next one...
+	    break;
+	}
+    }
+
+    hold_mods = modifiers;
+    return;
+}
