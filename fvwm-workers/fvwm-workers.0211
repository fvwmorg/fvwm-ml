			<int>0x2009</int>
			<int>0x200a</int>
			<int>0x200b</int>
			<int>0x3000</int>	<!-- CJK space -->
		</blank>
<!--
  Rescan configuration every 30 seconds when FcFontSetList is called
 -->
		<rescan>
			<int>30</int>
		</rescan>
	</config>

