diff -u -u -r1.196 add_window.c
--- fvwm/add_window.c	2001/01/09 10:04:27	1.196
+++ fvwm/add_window.c	2001/01/12 16:43:14
@@ -976,6 +976,9 @@
 void setup_icon(FvwmWindow *tmp_win, window_style *pstyle)
 {
   XTextProperty text_prop;
+  /* sacha@debian.org */
+  char** icon_list;
+  int icon_num;    
 
   /* find a suitable icon pixmap */
   if((tmp_win->wmhints) && (tmp_win->wmhints->flags & IconWindowHint))
@@ -1008,7 +1011,24 @@
   /* icon name */
   if (XGetWMIconName(dpy, tmp_win->w, &text_prop))
   {
-    tmp_win->icon_name = (char *)text_prop.value;
+  /* sacha@debian.org */
+    if (text_prop.encoding == XA_STRING)
+      tmp_win->icon_name = (char *)text_prop.value;
+    else
+    {
+      if (XmbTextPropertyToTextList(dpy, &text_prop, &icon_list, &icon_num) >= Success)
+      {
+	XFree(text_prop.value);
+	tmp_win->icon_name = *icon_list;
+      }
+      else
+      {
+        if(icon_list) XFreeStringList(icon_list);
+	XFree(text_prop.value);
+	if (XGetWMIconName(dpy, tmp_win->w, &text_prop))
+	  tmp_win->icon_name = (char *)text_prop.value;
+      }
+    }
   }
   else
   {

