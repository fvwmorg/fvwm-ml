Content-Description: Xsession.fw
Content-Disposition: attachment; filename="Xsession.fw"
Content-Transfer-Encoding: quoted-printable

#!/bin/ksh -x

exec

echo "using xsession.fw"

DEFAULT_XINITRC=3D"/usr/dt/config/Xinitrc.fw"
HOME_XINITRC=3D"$HOME/.xinitrc"
PATH=3D/usr/dt/bin:$PATH:/usr/openwin/bin

    if [ -z "$XINITRC" ]; then
        if [ -f $HOME_XINITRC ]; then
            XINITRC=3D$HOME_XINITRC
        else
            XINITRC=3D$DEFAULT_XINITRC
        fi
    fi

    if [ -f $XINITRC ]; then
        echo "using xinitrc file: $XINITRC"
        /bin/ksh $XINITRC
    else
        echo "xinitrc file: $XINITRC not found"
        if [ -f $DEFAULT_XINITRC ]; then
            echo "using xinitrc: $DEFAULT_XINITRC"
            /bin/ksh $DEFAULT_XINITRC =

        fi
    fi

echo "calling fvwm2...."

/opt/bin/fvwm2 > /tmp/fvwm2.$$ 2>&1

echo "end xsession.fw"

