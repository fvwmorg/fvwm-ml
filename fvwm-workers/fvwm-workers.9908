    if (!dpy_name)  dpy_name = ":0";
    if (!dpy_name[0]  ||  ':' == dpy_name[0])
      strcat( f_stem, hostname );  /* Put hostname before dpy if not there
*/
    strcat (f_stem, client);

  }else{
    f_stem = name;
  }


