#!/bin/sh

fvwm_ver=3D"fvwm-2.2.2"

if [ -d /usr/dt/config ]; then
  cp /opt/pkgs/${fvwm_ver}/Xsession.fw /usr/dt/config
  cp /opt/pkgs/${fvwm_ver}/Xinitrc.fw /usr/dt/config
  chown root /usr/dt/config/X*.fw
  chgrp bin  /usr/dt/config/X*.fw
fi

if [ -d /usr/dt/config/C/Xresources.d/ ]; then
  cp /opt/pkgs/${fvwm_ver}/Xresources.fw /usr/dt/config/C/Xresources.d/
fi

if [ -d /usr/dt/appconfig/icons/C ]; then
  cp /opt/pkgs/${fvwm_ver}/FVWMlogo.* /usr/dt/appconfig/icons/C
  chown root /usr/dt/appconfig/icons/C/FVWMlogo.*
  chgrp bin  /usr/dt/appconfig/icons/C/FVWMlogo.*
fi


/opt/sbin/mklinks.sh -f "${@}"

