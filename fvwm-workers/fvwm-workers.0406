-			window_list_entry *wle =
-				lookup_window_list_entry(body[0]);
-			if (wle->icon_name)
-			{
-				free(wle->icon_name);
-			}
-			wle->icon_name = safestrdup((char*)(&body[3]));
-		}
-		break;
-	case M_MINI_ICON:
-		{
-
-			MiniIconPacket *mip = (MiniIconPacket *)body;
-			window_list_entry *wle =
-				lookup_window_list_entry(mip->w);
-
-			if (wle->mini_icon)
-			{
-				free(wle->mini_icon);
