-	case M_DESTROY_WINDOW:
-		g_hash_table_remove(window_list_entries, &(body[0]));
-		break;
-	case M_VISIBLE_NAME:
-		{
-			window_list_entry *wle =
-				lookup_window_list_entry(body[0]);
-			if (wle->name)
-			{
-				free(wle->name);
-			}
-			wle->name = safestrdup((char *)(&body[3]));
-		}
