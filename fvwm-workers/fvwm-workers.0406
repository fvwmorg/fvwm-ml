 	enable_gtktest="$enableval", enable_gtktest=yes)
 if test x"$enable_gtktest" = xyes; then
-	AM_PATH_GTK(1.1.0,[
+	AM_PATH_GTK_2_0(2.0.0,[
 		FVWMGTK=FvwmGtk AC_SUBST(FVWMGTK)
