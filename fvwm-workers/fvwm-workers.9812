retrieving revision 1.3
diff -u -B -b -u -r1.3 borders.c
--- fvwm/borders.c	1998/11/06 02:22:50	1.3
+++ fvwm/borders.c	1998/12/03 17:42:35
@@ -1338,10 +1338,10 @@
     tmp_win->orig_wd = w;
     tmp_win->orig_ht = h;
   }
-  if (x >= Scr.MyDisplayWidth + Scr.VxMax - Scr.Vx-16)
-    x = Scr.MyDisplayWidth + Scr.VxMax -Scr.Vx - 16;
-  if (y >= Scr.MyDisplayHeight+Scr.VyMax - Scr.Vy -16)
-    y = Scr.MyDisplayHeight + Scr.VyMax - Scr.Vy - 16;
+  if (x >= Scr.MyDisplayWidth + Scr.VxMax - Scr.Vx + tmp_win->bw)
+    x = Scr.MyDisplayWidth + Scr.VxMax -Scr.Vx - 1 + tmp_win->bw;
+  if (y >= Scr.MyDisplayHeight + Scr.VyMax - Scr.Vy + tmp_win ->bw)
+    y = Scr.MyDisplayHeight + Scr.VyMax - Scr.Vy - 1 + tmp_win->bw;
 
   if((w != tmp_win->frame_width) || (h != tmp_win->frame_height))
     Resized = True;
@@ -1569,7 +1569,7 @@
     client_event.xconfigure.height =h-2*tmp_win->boundary_width -
       tmp_win->title_height;
 
-    client_event.xconfigure.border_width =tmp_win->bw;
+    client_event.xconfigure.border_width = 0;
     /* Real ConfigureNotify events say we're above title window, so ... */
     /* what if we don't have a title ????? */
     client_event.xconfigure.above = tmp_win->frame;
