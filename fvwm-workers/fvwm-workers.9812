diff -r -u fvwm.old/fvwm.c fvwm/fvwm.c
--- fvwm.old/fvwm.c	Wed Dec  9 09:49:09 1998
+++ fvwm/fvwm.c	Wed Dec  9 10:35:57 1998
@@ -42,6 +42,18 @@
 
 #define MAXHOSTNAME 255
 
+/*
+ * added 9-Dec-98 by lar3ry gensch -- allow direct specification of
+ * the location for the feedback window.  UNSET is default, to allow
+ * us to revert to the original behavior (set by MenuStyle).
+ */
+static enum {
+    FEEDBACK_UNSET,
+    FEEDBACK_CENTER,
+    FEEDBACK_TOPLEFT
+} feedback_opt = FEEDBACK_UNSET;
+
+Bool feedback_initialized = False;
 
 #ifndef lint
 static char sccsid[] = "@(#)fvwm.c " VERSION " " __DATE__ " fvwm";
@@ -432,7 +444,13 @@
   attributes.background_pixel = Scr.DefaultMenuFace->MenuColors.back;
   attributes.bit_gravity = NorthWestGravity;
   valuemask = (CWBorderPixel | CWBackPixel | CWBitGravity);
-  if(Scr.DefaultMenuFace->style != MWMMenu)
+
+  /*
+   * modified 9-Dec-98 by lar3ry gensch -- allow direct specification
+   * of the location for the feedback window.
+   */
+  if((feedback_opt == FEEDBACK_UNSET && Scr.DefaultMenuFace->style != MWMMenu) ||
+     (feedback_opt == FEEDBACK_TOPLEFT))
   {
     Scr.SizeWindow = XCreateWindow (dpy, Scr.Root,
                                     0, 0,
@@ -463,6 +481,8 @@
                                     (Visual *) CopyFromParent,
                                     valuemask, &attributes);
   }
+  feedback_initialized = True;
+
 #ifndef NON_VIRTUAL
   initPanFrames();
 #endif
@@ -1559,3 +1579,45 @@
     BlackoutWin = None;
   }
 } /* UnBlackoutScreen */
+
+/*
+ * added 9-Dec-98 by lar3ry gensch -- allow direct specification of
+ * the location for the feedback window.
+ */
+void feedback_func(XEvent *eventp,Window w,FvwmWindow *tmp_win,
+                   unsigned long context,char *action, int *Module)
+{
+  char *arg = NULL;
+
+  action = GetNextToken(action,&arg);
+  if (arg != NULL) {
+    if (strcasecmp(arg, "center") == 0)
+      feedback_opt = FEEDBACK_CENTER;
+    else if (strcasecmp(arg, "topleft") == 0)
+      feedback_opt = FEEDBACK_TOPLEFT;
+    else if (strcasecmp(arg, "menu") == 0)
+      feedback_opt = FEEDBACK_UNSET;
+    else {
+      fvwm_msg(ERR, "FeedbackWindow",
+	       "Options are \"center,\" \"menu,\" or \"topleft.\"");
+      feedback_opt = FEEDBACK_UNSET;
+    }
+  } else {
+    fvwm_msg(ERR,"FedbackWindow","FedbackWindow requires 1 argument");
+    feedback_opt = FEEDBACK_UNSET;
+  }
+
+  if (feedback_initialized) {
+    if((feedback_opt == FEEDBACK_UNSET && Scr.DefaultMenuFace->style != MWMMenu) ||
+     (feedback_opt == FEEDBACK_TOPLEFT))
+    {
+      XMoveWindow(dpy, Scr.SizeWindow, 0, 0);
+    } else {
+      XMoveWindow(dpy, Scr.SizeWindow,
+		  Scr.MyDisplayWidth/2 -
+		    (Scr.SizeStringWidth + SIZE_HINDENT*2)/2,
+		  Scr.MyDisplayHeight/2 -
+		    (Scr.StdFont.height + SIZE_VINDENT*2)/2);
+    }
+  }
+} /* feedback_func */
