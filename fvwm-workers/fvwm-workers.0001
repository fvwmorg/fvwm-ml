       * pixel of the original colour shine through. */
!     sum = (w_dout + w_hiout + w_trout + w_trin + w_shin + w_din);
      trim = sum - t->boundary_width;
    }
    if (DFS_HAS_NO_INSET(*borderstyle))
    {
+     sum -= (w_shin + w_din + w_trin); /* added by dse */
+     trim -= (w_shin + w_din + w_trin); /* added by dse */
      w_shin = 0;
!     w_din = 0; /* added by dse */
