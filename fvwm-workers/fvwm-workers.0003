#include <X11/Xlib.h>

main()
{
   Display *dpy;
   Window   win;

   dpy = XOpenDisplay("");
   win = XCreateSimpleWindow( dpy, XDefaultRootWindow(dpy),
      0,0, 1,1, 0, 0, 0 );

   XMapWindow( dpy, win );
   XFlush( dpy );

   sleep(2);

   XMoveResizeWindow( dpy, win, 0,0, 800,600 );
   XFlush( dpy );

   sleep(2);

   XCloseDisplay(dpy);
}

