!             if (flags & SHOW_GEOMETRY)
              AddToMenu(mr, NULL, NULL, FALSE, FALSE);
            last_desk_displayed = t->Desk;
          }
--- 339,345 ----
          if (t->Desk != last_desk_displayed)
          {
            if (last_desk_displayed != INT_MIN)
!             if ((flags & SHOW_GEOMETRY) || (flags & SHOW_INFONOTGEO))
              AddToMenu(mr, NULL, NULL, FALSE, FALSE);
            last_desk_displayed = t->Desk;
