@@ -166,6 +166,7 @@
 int main(int argc, char **argv)
 {
   char *temp, *s;
+  unsigned long mask;
 #ifdef HAVE_SIGACTION
   struct sigaction  sigact;
 #endif
@@ -251,15 +252,17 @@
   /* Request a list of all windows,
