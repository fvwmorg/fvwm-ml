+ 	       matches[num_match - 1].wm_command = 
+ 		 (char **) malloc (matches[num_match - 1].wm_command_count *
+ 				   sizeof (char *));
+ 	       pos = 0;
+ 	       for (i = 0; i < matches[num_match - 1].wm_command_count; i++) 
