+ static void
+ SaveWindowStates(void)
+ {
+   FvwmWindow              *ewin;
+   int                 i, num, x, y;
+   FILE               *f;
+   char                s[4096];
+   int uid;
+   struct passwd      *pwd;
+ 
+   pwd = getpwuid(getuid());
+ 
+   if (sm_client_id)
+     snprintf(s, sizeof(s), "%s/.fvwm_clients_save.%s.%i", pwd->pw_dir, sm_client_id, Scr.screen);
+   else
+     snprintf(s, sizeof(s), "%s/.fvwm_clients_save.%i", pwd->pw_dir, Scr.screen);
+ 
+   f = fopen(s, "w");
+   if (f)
+     {
+       for (ewin=Scr.FvwmRoot.stack_next; ewin!=&Scr.FvwmRoot; ewin=ewin->stack_next) 
+ 	{
+ 	  if (ewin->session_id)
+ 	    {
+ 	      /* MUST have SESSION_ID to even try session manage it */
+ 	      fprintf(f, "[CLIENT] %i %i %i %i %i %i %i %i %i %i %i\n",
+ 		      ewin->orig_x - ewin->old_bw + ewin->bw,
+                       ewin->orig_y - ewin->old_bw + ewin->bw,
+ 		      ewin->orig_wd - 2*ewin->boundary_width , 
+                       ewin->orig_ht  - 2*ewin->boundary_width - ewin->title_height, 
+                       ewin->icon_x_loc,
+                       ewin->icon_y_loc,
+ 		      ewin->Desk, 
+                       !!(ewin->flags & ICONIFIED), 
+ 		      !!(ewin->buttons & WSHADE), 
+                       !!(ewin->flags & MAXIMIZED),
+ 		      !!(ewin->flags & STICKY) );
+ 	      fprintf(f, "  [SESSION_ID] %s\n", ewin->session_id);
+ 	      if (ewin->class.res_name)
+ 		fprintf(f, "  [NAME] %s\n", ewin->class.res_name);
+ 	      if (ewin->class.res_class)
+ 		fprintf(f, "  [CLASS] %s\n", ewin->class.res_class);
+ 	      if (ewin->role)
+ 		fprintf(f, "  [ROLE] %s\n", ewin->role);
+ 	      if (ewin->command)
+ 		fprintf(f, "  [COMMAND] %s\n", ewin->command);
+ 	      if (ewin->name)
+ 		fprintf(f, "  [TITLE] %s\n", ewin->name);
+ 	    }
+ 	}
+     }
+   fclose(f);
+ }
+ 
+ char *duplicate(char *s)
+ {
+   int l;
+   char *r;
+ 
+   if (!s) return NULL;
+   l = strlen(s);
+   r = (char *) malloc (sizeof(char)*(l+1));
+   strncpy(r, s, l+1);
+   return r;
+ }
+ 
+ static void
+ LoadWindowStates(void)
+ {
+    FILE               *f;
+    char                s[4096], s1[4096];
+ 
+   struct passwd      *pwd;
+ 
+   fprintf(stderr, "LoadWindowStates...\n");
+   pwd = getpwuid(getuid());
+ 
+    if (sm_client_id)
+      snprintf(s, sizeof(s), "%s/.fvwm_clients_save.%s.%i", pwd->pw_dir, sm_client_id, Scr.screen);
+    else
+      snprintf(s, sizeof(s), "%s/.fvwm_clients_save.%i", pwd->pw_dir, Scr.screen);
+ 
+    f = fopen(s, "r");
+    if (f)
+      {
+        while (fgets(s, sizeof(s), f))
+ 	 {
+ 	   sscanf(s, "%4000s", s1);
+ 	   if (!strcmp(s1, "[CLIENT]"))
+ 	     {
+ 	       num_match++;
+ 	       matches = realloc(matches, sizeof(Match) * num_match);
+ 	       matches[num_match - 1].session_id = NULL;
+ 	       matches[num_match - 1].name = NULL;
+ 	       matches[num_match - 1].class = NULL;
+ 	       matches[num_match - 1].role = NULL;
+ 	       matches[num_match - 1].used = 0;
+ 	       sscanf(s, "%*s %i %i %i %i %i %i %i %i %i %i %i",
+ 		      &(matches[num_match - 1].x),
+ 		      &(matches[num_match - 1].y),
+ 		      &(matches[num_match - 1].w),
+ 		      &(matches[num_match - 1].h),
+ 		      &(matches[num_match - 1].icon_x),
+ 		      &(matches[num_match - 1].icon_y),
+ 		      &(matches[num_match - 1].desktop),
+ 		      &(matches[num_match - 1].iconified),
+ 		      &(matches[num_match - 1].shaded),
+ 		      &(matches[num_match - 1].maximized),
+ 		      &(matches[num_match - 1].sticky)
+ 		      );
+ 	     }
+ 	   else if (!strcmp(s1, "[SESSION_ID]"))
+ 	     {
+ 	       sscanf(s, "%*s %4000s", s1);
+ 	       matches[num_match - 1].session_id = duplicate(s1);
+                fprintf(stderr,"[SESSION_ID] %s...\n", s1);
+ 	     }
+ 	   else if (!strcmp(s1, "[NAME]"))
+ 	     {
+ 	       sscanf(s, "%*s %4000s", s1);
+ 	       matches[num_match - 1].name = duplicate(s1);
+ 	     }
+ 	   else if (!strcmp(s1, "[CLASS]"))
+ 	     {
+ 	       sscanf(s, "%*s %4000s", s1);
+ 	       matches[num_match - 1].class = duplicate(s1);
+ 	     }
+ 	   else if (!strcmp(s1, "[ROLE]"))
+ 	     {
+ 	       sscanf(s, "%*s %4000s", s1);
+ 	       matches[num_match - 1].role = duplicate(s1);
+ 	     }
+ 	   
+ 	 }
+        fclose(f);
+      }
+ }
+ 
+ Bool matchWin(FvwmWindow *w, Match *m)
+ {
+    if (!w->session_id)
+      return 0; 
+    if (strcmp(w->session_id, m->session_id))
+      return 0;
+    if (w->class.res_name && m->name &&
+        strcmp(w->class.res_name, m->name))
+      return 0;
+    if (w->class.res_class && m->class &&
+        strcmp(w->class.res_class, m->class))
+      return 0;
+    if (w->role && m->role &&
+        strcmp(w->role, m->role))
+      return 0;
+    return 1;
+ }
+ 
+ /*
+    This routine (potentially) changes the flags STARTICONIC,
+    startShaded, startMaximized and STICKY and the Desk and
+    attr.x, .y, .width, .height entries. It also changes the
+    stack_before pointer to return information about the
+    desired stacking order. It expects the stacking order
+    to be set up correctly beforehand! 
+  */
+ void
+ MatchWinToSM(FvwmWindow *ewin)
+ {
+    int                 i, j;
+    FvwmWindow *t;
+ 
+    fprintf(stderr,"MatchWinToSM %s %s...\n", ewin->session_id, ewin->class.res_name);
+    if (!ewin->session_id)
+      return;
+    for (i = 0; i < num_match; i++)
+      {
+        fprintf(stderr, "trying %d...\n",i);
+        if (!matches[i].used && matchWin(ewin, &matches[i]))
+ 	     {
+                fprintf(stderr,"hit! %d %d %d %d  %d %d  %d %d %d %d %d\n",
+                  matches[i].x, matches[i].y, matches[i].w, matches[i].h,
+                  matches[i].icon_x, matches[i].icon_y, matches[i].desktop,
+                  matches[i].iconified, matches[i].shaded, matches[i].maximized,
+                  matches[i].sticky);
+ 	       matches[i].used = 1;
+ 	       if (matches[i].iconified) {
+                  /* 
+                     ICON_MOVED is necessary to make fvwm use icon_[xy]_loc 
+                     for icon placement
+                  */   
+                  ewin->flags |= STARTICONIC|ICON_MOVED;
+                } else { 
+                  ewin->flags &= ~STARTICONIC;
+                }
+                ewin->tmpflags.startShaded = !!matches[i].shaded;
+                ewin->tmpflags.startMaximized = !!matches[i].maximized;
+ 	       if (matches[i].sticky) {
+                  ewin->flags |= STICKY;
+ 	       } else {
+                  ewin->flags &= ~STICKY;
+                }
+ 	       if (!(ewin->flags & STICKY))
+ 		 ewin->Desk = matches[i].desktop;
+                ewin->attr.x = matches[i].x;
+                ewin->attr.y = matches[i].y;
+                ewin->attr.width = matches[i].w;
+                ewin->attr.height = matches[i].h;
+                ewin->icon_x_loc = matches[i].icon_x;
+                ewin->icon_y_loc = matches[i].icon_y;
+                 
+                /* Find the window to stack this one below. */
+ 
+                fprintf(stderr, "find stack...\n");
+                for (j = i-1; j >= 0; j--) {
+                
+ 
+                  /* matches are sorted in stacking order */
+ 
+                  if (matches[j].used) {
+                    fprintf(stderr, "candidate %d...\n",j);
+ 
+                    for (t = Scr.FvwmRoot.next; t != NULL; t = t->next) { 
+                      if (matchWin(t, &matches[j])) {
+                        fprintf(stderr,"hit!\n");
+                        ewin->stack_next->stack_prev = ewin->stack_prev;
+                        ewin->stack_prev->stack_next = ewin->stack_next;
+ 
+                        ewin->stack_prev = t;
+                        ewin->stack_next = t->stack_next;
+                        ewin->stack_prev->stack_next = ewin;
+                        ewin->stack_next->stack_prev = ewin;
+                        return;
+                      }
+                    }
+                  } 
+                }
+ 	       return;
+ 	     }
+ 	 }
+ }
+ 
+ static void
+ callback_save_yourself2(SmcConn smc_conn, SmPointer client_data)
+ {
+   char                master_flag;
+   
+   set_save_props(smc_conn);
+   master_flag = (master_pid == getpid())? 1 : 0;
+   if (master_flag)
+     {
+       SaveWindowStates();
+       /*
+       if (mode.autosave)
+ 	SaveUserControlConfig();
+       else
+ 	DeleteUserControlConfig();
+       */
+       /*
+       SetEInfoOnAll();
+       */
+     }
+   SmcSaveYourselfDone(smc_conn, True);
+   client_data = NULL;
+ }
+ 
+ static void
+ callback_save_yourself(SmcConn smc_conn, SmPointer client_data,
+ 		       int save_style, Bool shutdown, int interact_style,
+ 		       Bool fast)
+ {
+    SmcRequestSaveYourselfPhase2(smc_conn, callback_save_yourself2, NULL);
+    client_data = NULL;
+    save_style = 0;
+    shutdown = False;
+    interact_style = 0;
+    fast = False;
+ }
+ 
+ static void
+ callback_die(SmcConn smc_conn, SmPointer client_data)
+ {
+    char                sss[2048];
+    char                master_flag;
+    XEvent ev;
+    FvwmWindow *t;
+ 
+    setsid();
+    /* 
+       I believe this to be a bug in Enlightenment: 
+       SaveWindowStates();
+    */ 
+    master_flag = (master_pid == getpid())? 1 : 0;
+    if (!master_flag)
+       kill(master_pid, SIGTERM);
+    /*
+    if (dpy)
+       SetEInfoOnAll();
+    */
+   {
+     /* This part taken from Done in fvwm.c */
+     MenuRoot *mr;
+ 
+ #ifndef NON_VIRTUAL
+     MoveViewport(0,0,False);
+ #endif
+ 
+     mr = FindPopup("ExitFunction");
+     if(mr != NULL)
+       ExecuteFunction("Function ExitFunction",NULL,&Event,C_ROOT,-1);
+ 
+     /* Close all my pipes */
+     ClosePipes();
+ 
+     /* RestoreWithdrawnPosition should handle this... */
+     for (t = Scr.FvwmRoot.next; t != NULL; t = t->next) {
+       if (!(t->flags & ICONIFIED)) {
+         if (t->flags & MAXIMIZED)
+           Maximize(&ev, t->w, t, C_WINDOW, "", 0);
+         if (t->buttons & WSHADE)
+           WindowShade(&ev, t->w, t, C_WINDOW, "", 0);
+       }
+     }
+     Reborder ();
+    }
+    MyXUngrabServer(dpy);
+    UngrabEm();
+    XCloseDisplay(dpy);
+    /*
+    if (mode.autosave)
+       SaveUserControlConfig();
+    else
+       DeleteUserControlConfig();
+    */
+    SmcSaveYourselfDone(smc_conn, True);
+    SmcCloseConnection(smc_conn, 0, NULL);
+    client_data = NULL;
+ }
+ 
+ static void
+ callback_save_complete(SmcConn smc_conn, SmPointer client_data)
+ {
+    smc_conn = NULL;
+    client_data = NULL;
+ }
+ 
+ static void
+ callback_shutdown_cancelled(SmcConn smc_conn, SmPointer client_data)
+ {
+    SmcSaveYourselfDone(smc_conn, False);
+    client_data = NULL;
+ }
+ 
+ void
+ SessionInit(void)
+ {
+    static SmPointer    context;
+    SmcCallbacks        callbacks;
+    char               *client_id = NULL;
+ 
+    callbacks.save_yourself.callback = callback_save_yourself;
+    callbacks.die.callback = callback_die;
+    callbacks.save_complete.callback = callback_save_complete;
+    callbacks.shutdown_cancelled.callback = callback_shutdown_cancelled;
+ 
+    callbacks.save_yourself.client_data =
+       callbacks.die.client_data =
+       callbacks.save_complete.client_data =
+       callbacks.shutdown_cancelled.client_data = (SmPointer) NULL;
+    if (getenv("SESSION_MANAGER"))
+      {
+ 	char                error_string_ret[4096] = "";
+ 
+ 	sm_conn = SmcOpenConnection(NULL, &context, SmProtoMajor, SmProtoMinor,
+ 				    SmcSaveYourselfProcMask | SmcDieProcMask |
+ 				    SmcSaveCompleteProcMask |
+ 				    SmcShutdownCancelledProcMask,
+ 				    &callbacks, sm_client_id, &client_id,
+ 				    4096, error_string_ret);
+ 	if (error_string_ret[0])
+ 	  fprintf(stderr, "While connecting to session manager:\n%s.",
+ 		  error_string_ret);
+      }
+    if (sm_conn)
+      {
+ 	IceConn             ice_conn;
+ 	int                 restarted = 0;
+ 
+ 	if (sm_client_id == NULL)
+ 	  sm_client_id = client_id;
+ 	else if (!strcmp(sm_client_id, client_id) != 0)
+ 	  {
+ 	    free(sm_client_id);
+ 	    sm_client_id = client_id;
+ 	  }
+ 	else
+ 	  {
+ 	    free(client_id);
+ 	    restarted = 1;
+ 	  }
+ 	ice_conn = SmcGetIceConnection(sm_conn);
+ 	sm_fd = IceConnectionNumber(ice_conn);
+      }
+    LoadWindowStates();
+ }
+ 
+ void
+ ProcessICEMSGS(void)
+ {
+    IceProcessMessagesStatus status;
+ 
+    if (sm_fd < 0)
+       return;
+    status = IceProcessMessages(SmcGetIceConnection(sm_conn), NULL, NULL);
+    if (status == IceProcessMessagesIOError)
+      {
+        fprintf(stderr, "SM died!\n");
+ 	sm_conn = NULL;
+ 	sm_fd = -1;
+      }
+ }
+ 
+ int
+ GetSMfd(void)
+ {
+    return sm_fd;
+ }
+ 
+ void               *
+ AtomGet(Window win, Atom to_get, Atom type, int *size)
+ {
+    unsigned char      *retval;
+    Atom                type_ret;
+    unsigned long       bytes_after, num_ret;
+    int                 format_ret;
+    long                length;
+    void               *data;
+ 
+    retval = NULL;
+    length = 0x7fffffff;
+    XGetWindowProperty(dpy, win, to_get, 0,
+ 		      length,
+ 		      False, type,
+ 		      &type_ret,
+ 		      &format_ret,
+ 		      &num_ret,
+ 		      &bytes_after,
+ 		      &retval);
+    if ((retval) && (num_ret > 0) && (format_ret > 0))
+      {
+ 	data = malloc(num_ret * (format_ret >> 3));
+ 	if (data)
+ 	  memcpy(data, retval, num_ret * (format_ret >> 3));
+ 	XFree(retval);
+ 	*size = num_ret * (format_ret >> 3);
+ 	return data;
+      }
+    return NULL;
+ }
+ 
+ void
+ SessionGetInfo(FvwmWindow* ewin)
+ {
+    Atom                a;
+    char               *s;
+    int                 size;
+ 
+    fprintf(stderr, "Getting window info...\n");
+    if (ewin->session_id)
+      {
+ 	free(ewin->session_id);
+ 	ewin->session_id = NULL;
+      }
+    a = XInternAtom(dpy, "SM_CLIENT_ID", False);
+    s = AtomGet(ewin->w, a, XA_STRING, &size);
+    if (s)
+      {
+ 	ewin->session_id = (char *)malloc(sizeof(char)*(size + 1));
+ 	memcpy(ewin->session_id, s, size);
+ 	ewin->session_id[size] = 0;
+         fprintf(stderr, "session_id == %s\n",ewin->session_id);
+ 	free(s);
+ 	return;
+      }
+    s = AtomGet(ewin->wmhints->window_group, a, XA_STRING, &size);
+    if (s)
+      {
+        ewin->session_id = (char *)malloc(sizeof(char)*(size + 1));
+        memcpy(ewin->session_id, s, size);
+        ewin->session_id[size] = 0;
+        fprintf(stderr, "group leaders session_id == %s\n",ewin->session_id);
+        free(s);
+        return;
+      }
+ }
+ 
+ void
+ SetSMID(char *smid)
+ {
+   fprintf(stderr, "setting smid to %s...\n", smid);
+   if (sm_client_id)
+     free(sm_client_id);
+   sm_client_id = NULL;
+   sm_client_id = duplicate(smid);
+ }
+ 
+ char               *
+ GetSMID(void)
+ {
+   const char         *fake = "NONE";
+   
+   if (!sm_client_id)
+     return (char *)fake;
+   return sm_client_id;
+ }


