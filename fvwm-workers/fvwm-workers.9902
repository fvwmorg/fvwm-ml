+    if (tmp_win && !(tmp_win->buttons & WSHADE))
     { /* send configure notify event for Eterm */
       XEvent client_event;
       client_event.type = ConfigureNotify;
@@ -669,7 +669,7 @@
 
 	}
 #ifndef NO_ETERM_SUPPORT
