+     return;
+   
+   if(tmp_win == NULL)
+     return;
+ 
+   n = GetIntegerArguments(action, NULL, val, 2);
+ 
+   layer = tmp_win->layer;
+   if ((n == 1) || 
+       ((n == 2) && (val[0] != 0))) 
+     {
+       layer += val[0];
+     }
+   else if ((n == 2) && (val[1] >= 0)) 
+     {
+       layer = val[1];
+     }
+   else
+     {
+       /* a hack: Layer 0 -1 goes back to the "default" layer */
