+ void 
+ LoadGlobalState(char *filename)
+ {
+    FILE               *f;
+    char                s[4096], s1[4096];
+    int i1, i2, i3, i4, i5, i6, i7, i8;
+ 
+    f = fopen(filename, "r");
+    if (f)
+      {
+        while (fgets(s, sizeof(s), f))
+ 	 {
+            i1 = 0; i2 = 0; i3 = 0; i4 = 0; i5 = 0; i6 = 0;
+            i7 = 0; i8 = 0;
+ 	   sscanf(s, "%4000s", s1);
+ 	   if (!strcmp(s1, "[DESKTOP]")) 
+              {
+ 	       sscanf(s, "%*s %i", &i1);
+                changeDesks(i1);
+              }
+            else if (!strcmp(s1, "[VIEWPORT]"))
+              {
+                sscanf(s, "%*s %i %i %i %i", &i1, &i2, &i3, &i4);
+                Scr.VxMax = i3;
+                Scr.VyMax = i4;
+                MoveViewport(i1, i2, True);
+              }
+            else if (!strcmp(s1, "[SCROLL]"))
+              {
+                sscanf(s, "%*s %i %i %i %i %i %i", &i1, &i2, &i3, &i4, &i5, &i6);
+                Scr.EdgeScrollX = i1; 
+                Scr.EdgeScrollY = i2;
+                Scr.ScrollResistance = i3;
+                Scr.MoveResistance = i4;
+                if (i5) Scr.flags |= EdgeWrapX;
+                else Scr.flags &= ~EdgeWrapX;
+                if (i6) Scr.flags |= EdgeWrapY;
+                else Scr.flags &= ~EdgeWrapY;
+              }
+            else if (!strcmp(s1, "[SNAP]"))
+              {
+                sscanf(s, "%*s %i %i %i %i", &i1, &i2, &i3, &i4);
+                Scr.SnapAttraction = i1;
+                Scr.SnapMode = i2;
+                Scr.SnapGridX = i3;
+                Scr.SnapGridY = i4;
+              }
+            else if (!strcmp(s1, "[MISC]"))
+              {
+                sscanf(s, "%*s %i %i %i %i %i %i %i %i", &i1, &i2, &i3, &i4,
+                       &i5, &i6, &i7, &i8);
+                Scr.ClickTime = i1;
+                Scr.ColormapFocus = i2;
+                Scr.ColorLimit = i3;
+                Scr.SmartPlacementIsClever = i4;
+                Scr.ClickToFocusPassesClick = i5;
+                Scr.ClickToFocusRaises = i6;
+                Scr.MouseFocusClickRaises = i7;
+                Scr.StipledTitles = i8;
+              }
+            else if (!strcmp(s1, "[STYLE]"))
+              {
+                sscanf(s, "%*s %i %i %i %i", &i1, &i2, &i3, &i4, &i5, &i6);
+                Scr.go.ModifyUSP = i1;
+                Scr.go.CaptureHonorsStartsOnPage = i2;
+                Scr.go.RecaptureHonorsStartsOnPage = i3;
+                Scr.go.ActivePlacementHonorsStartsOnPage = i4;
+                Scr.gs.EmulateMWM = i5; 
+                Scr.gs.EmulateWIN = i6; 
+              }
+          }
+       fclose(f);
+    }
+ }
