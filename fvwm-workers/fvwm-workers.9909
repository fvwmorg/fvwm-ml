Neil, please replace the beginning of the process_message function
in FvwmPager.c with this code and record the output when your problem
occurs.

void process_message( FvwmPacket* packet )
{
  unsigned long type = packet->type;
  unsigned long length = packet->size;
  unsigned long* body = packet->body;

  switch (type)
    {
    case M_ADD_WINDOW:
fprintf(stderr,"add window: 0x%x\n", packet->body[0]);
      list_configure(body);
      break;
    case M_CONFIGURE_WINDOW:
fprintf(stderr,"configure window: 0x%x\n", packet->body[0]);
      list_configure(body);
      break;
    case M_DESTROY_WINDOW:
fprintf(stderr,"destroy window: 0x%x\n", packet->body[0]);
      list_destroy(body);
      break;
    case M_FOCUS_CHANGE:
      list_focus(body);
      break;
    case M_NEW_PAGE:
      list_new_page(body);
      break;
    case M_NEW_DESK:
      list_new_desk(body);
      break;
    case M_RAISE_WINDOW:
      list_raise(body);
      break;
    case M_LOWER_WINDOW:
      list_lower(body);
      break;
    case M_ICONIFY:
fprintf(stderr,"iconify: 0x%x\n", packet->body[0]);
    case M_ICON_LOCATION:
      list_iconify(body);
      break;
    case M_DEICONIFY:
fprintf(stderr,"deiconify: 0x%x\n", packet->body[0]);
      list_deiconify(body);
      break;

Bye

