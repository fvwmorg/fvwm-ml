+/****************************************************************************
+ *  Like NextFunc, but if more than Scr.CoolSecs seconds have gone by since
+ *  the last switch, move the current focus window to the top of the stack.
+ *  This acts like MS Windows "Cool Switching" (they've only had it a decade
+ *  now :-).  Unfortunately, there was no way to get the
+ *
+ *  Windows uses the release of the "Alt" key to signal end of switch.
+ *  I wasn't able to figure out how to do that, so I used a simple timing
+ *  mechanism.  Use the config directive "CoolNextSecs" to set.
+ *
+ *  CoolNext changes by Sean Reifschneider <jafo@tummy.com>, 1998-09
+ *  Copyright (c) 1998, tummy.com, ltd.  All Rights Reserved.
+ *  Released under the Mozilla Public License
+ *  (http://www.mozilla.org/NPL/MPL-1.0.html), for inclusion in FVWM.
+ *
+ *  FVWM didn't seem to include any license, so I errored on the safe
+ *  side of using the MPL.  If this causes any problems, let's talk...
+ */
+
+void CoolNextFunc(XEvent *eventp,Window junk,FvwmWindow *tmp_win,
+              unsigned long context, char *action,int* Module)
+{
+FvwmWindow *found;
+char *restofline;
+static time_t lastSwitch = 0;
+time_t now = time(NULL);
+FvwmWindow *tmp, *fw;
+
+
+if (Scr.CoolSecs > 0 && now - lastSwitch > Scr.CoolSecs && Scr.Focus) {
+	WinListRemove(Scr.Focus);
+	WinListAppend(Scr.Focus);
+	}
+lastSwitch = now;
+
+NextFunc(eventp, junk, tmp_win, context, action, Module);
+}
+
+
+/******************************************************************************/
+/*  set the number of seconds before a Cool Next will re-arrange window list  */
+/*  Sean Reifschneider <jafo@tummy.com> 1998-09                               */
+
+void CoolSetSecs(XEvent *eventp,Window w,FvwmWindow *tmp_win,
+              unsigned long context, char *action,int* Module)
+{
+long val1;
+int val1_unit;
+
+
+if (GetOneArgument(action, &val1, &val1_unit) != 1) {
+	fvwm_msg(ERR, "CoolSetSecs", "Requires 1 argument");
+	return;
+	}
+
+Scr.CoolSecs = val1;
 }
 
 void PrevFunc(XEvent *eventp,Window junk,FvwmWindow *tmp_win,
