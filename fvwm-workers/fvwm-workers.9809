    keysym = XLookupKeysym(&Event->xkey,0);
  
***************
*** 447,472 ****
    switch(keysym)
      {
      case XK_Up:
      case XK_k:
      case XK_p:
!       y_move = -move_size;
        break;
      case XK_Down:
      case XK_n:
      case XK_j:
!       y_move = move_size;
        break;
      case XK_Left:
