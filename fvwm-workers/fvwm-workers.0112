+          text = GetNextToken(text, &ltok);
+          if (ltok)
+          {
+            got_number = (sscanf(ltok, "%d", &layer) == 1);
+            free (ltok);
+          }
+          else
+          {
+            got_number = 0;
+          }
+          if (!ltok || !got_number)
+          {
+            fvwm_msg(ERR, "ButtonStyle",
+                     "could not read integer value for layer -- line: %s",
+                     text);
+          }
+          else
+          {
+            SetLayerButtonFlag(layer, multi, set, decor, tb);
+          }
+          
+        }
 	else
 	{
 	  fvwm_msg(ERR, "ButtonStyle",
