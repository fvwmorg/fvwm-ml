diff -u -r1.32 stack.c
--- fvwm/stack.c	2000/01/25 14:38:59	1.32
+++ fvwm/stack.c	2000/05/14 20:36:37
@@ -216,6 +216,8 @@
 static Bool must_move_transients(FvwmWindow *t, Bool do_lower,
 				 Bool *found_transient)
 {
+  int last_layer = -1;
+  int out_of_order = 0;
   *found_transient = False;
 
   /* raise */
@@ -227,6 +229,10 @@
 
     for (q = Scr.FvwmRoot.stack_next; q != &Scr.FvwmRoot; q = q->stack_next)
     {
+      if ((last_layer > -1) && (last_layer < q->layer))
+      {
+	out_of_order++;
+      }
       if (t->layer < q->layer)
       {
 	/* We're not interested in higher layers. */
@@ -259,9 +265,13 @@
 	 * lower. */
 	return True;
       }
+      last_layer = q->layer;
     }
   }
-  return False;
+  if (out_of_order)
+    return True;
+  else
+    return False;
 }
 
 static void RaiseOrLowerWindow(
