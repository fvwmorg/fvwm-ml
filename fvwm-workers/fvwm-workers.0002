 filename="prtest.c"

#include <stdio.h>
#include <X11/Xlib.h>

main()
{
  Display *dpy;
  int scr;
  Window win;
  XSetWindowAttributes xswa;

  dpy = XOpenDisplay(NULL);
  scr = DefaultScreen(dpy);
  win = XCreateSimpleWindow(dpy, DefaultRootWindow(dpy), 1000, 0, 100, 100, 1,
                            BlackPixel(dpy, scr), WhitePixel(dpy, scr));
  xswa.background_pixmap = ParentRelative;
  xswa.override_redirect = False;
  XChangeWindowAttributes(dpy, win, CWBackPixmap | CWOverrideRedirect, &xswa);
  XMapWindow(dpy, win);
  XSync(dpy, False);
  sleep(600);
}
