The following diff for events.c allows unbound mouse actions on the root
window to be forwarded to Gnome desktop.  This seems desirable to me,
as this is what the Gnome WM spec recommends.



--jEy+Ab3MPR
Content-Type: text/plain
Content-Description: diffs for events.c
Content-Disposition: inline;
	filename="events.diff"
Content-Transfer-Encoding: 7bit

*** events.c	Sat Oct 16 05:32:52 1999
--- ../../../fvwm/fvwm/events.c	Thu Nov  4 17:18:30 1999
***************
*** 1421,1430 ****
  			Event.xbutton.state, GetUnusedModifiers(), Context,
  			MOUSE_BINDING);
  #endif /* HAVE_STROKE */
!   if (action != NULL)
    {
      ExecuteFunction(action, Tmp_win, &Event, Context, -1, EXPAND_COMMAND);
    }
  
    OldPressedW = PressedW;
    PressedW = None;
--- 1421,1442 ----
  			Event.xbutton.state, GetUnusedModifiers(), Context,
  			MOUSE_BINDING);
  #endif /* HAVE_STROKE */
!   if (action != NULL && (strlen(action) != 0))
    {
      ExecuteFunction(action, Tmp_win, &Event, Context, -1, EXPAND_COMMAND);
    }
+ #ifdef GNOME
+   else
+   {
+ /*-----------------------------------------------------------------------
+     do gnome buttonpress forwarding if win == root
+  -----------------------------------------------------------------------*/
+       if (Scr.Root == Event.xany.window)
+       {
+           GNOME_ProxyButtonEvent(&Event);
+       }
+   }
+ #endif
  
    OldPressedW = PressedW;
    PressedW = None;
***************
*** 1480,1487 ****
  /* DEBUG printfs
     printf ("action is %p\n", action);
  */
!    if (action != NULL)
       ExecuteFunction(action,Tmp_win, &Event,Context,-1, EXPAND_COMMAND);
  }
  
  /***********************************************************************
--- 1492,1514 ----
  /* DEBUG printfs
     printf ("action is %p\n", action);
  */
!    if (action != NULL && strlen(action) != 0)
!    {
       ExecuteFunction(action,Tmp_win, &Event,Context,-1, EXPAND_COMMAND);
+    }
+ #ifdef GNOME
+    else
+    {
+ /*-----------------------------------------------------------------------
+     do gnome buttonpress forwarding if win == root
+  -----------------------------------------------------------------------*/
+       if (Scr.Root == Event.xany.window)
+       {
+           GNOME_ProxyButtonEvent(&Event);
+       }
+    }
+ #endif
+ 
  }
  
  /***********************************************************************

--jEy+Ab3MPR
