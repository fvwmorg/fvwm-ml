Today I had a look at afterstep and noticed that it handles 
shaded windows different than fvwm2. It subsumes the shaded
state among iconic. This makes a lot of sense. fvwm currently
makes it normal state.

The following patch changes fvwm to do the same, and allow 
all transitions between iconified, normal and shaded.

This even avoids a bug in afterstep: If a client deiconifies
a shaded window, afterstep doesn't unshade it. We do.


--- icons.c.last	Fri Jan 15 01:50:38 1999
+++ icons.c	Fri Jan 15 01:50:07 1999
@@ -873,6 +873,10 @@
   return;
 }
 
+#ifdef WINDOWSHADE
+void UnShade(FvwmWindow *tmp_win);
+#endif
+
 /****************************************************************************
  *
  * Iconifies the selected window
@@ -886,6 +890,12 @@
 
   if(!tmp_win)
     return;
+
+#ifdef WINDOWSHADE
+  if (tmp_win->buttons & WSHADE) {
+    UnShade(tmp_win);
+  }
+#endif
 
   XGetWindowAttributes(dpy, tmp_win->w, &winattrs);
   eventMask = winattrs.your_event_mask;
--- builtins.c.last	Fri Jan 15 01:52:00 1999
+++ builtins.c	Fri Jan 15 01:50:00 1999
@@ -436,49 +436,98 @@
  *
  *  WindowShade -- shades or unshades a window (veliaa@rpi.edu)
  *
- *  Args: 1 -- force shade, 2 -- force unshade  No Arg: toggle
+ *  Args: > 0 -- force shade, < 0 -- force unshade  No Arg: toggle
  *
  ***********************************************************************/
+/* tmp_win should be in normal state for this to work */
+void UnShade (FvwmWindow *tmp_win) 
+{    
+  XWindowAttributes winattrs = {0};
+  unsigned long eventMask;
+
+  tmp_win->buttons &= ~WSHADE;
+
+  XGetWindowAttributes(dpy, tmp_win->w, &winattrs);
+  eventMask = winattrs.your_event_mask;
+
+  tmp_win->flags |= MAPPED;
+  XSelectInput(dpy, tmp_win->w, eventMask & ~StructureNotifyMask);
+  XMapWindow(dpy, tmp_win->w);
+  XSelectInput(dpy, tmp_win->w, eventMask);
+
+  SetupFrame(tmp_win,
+	     tmp_win->frame_x,
+	     tmp_win->frame_y,
+	     tmp_win->orig_wd,
+	     tmp_win->orig_ht,
+	     True);
+
+  SetMapStateProp(tmp_win, NormalState);
+
+  BroadcastPacket(M_DEWINDOWSHADE, 3,
+		  tmp_win->w, tmp_win->frame, (unsigned long)tmp_win);
+}
+
+
+void Shade (FvwmWindow *tmp_win)
+{
+  XWindowAttributes winattrs = {0};
+  unsigned long eventMask;
+
+  tmp_win->buttons |= WSHADE;
+  
+  SetupFrame(tmp_win,
+	     tmp_win->frame_x,
+	     tmp_win->frame_y,
+	     tmp_win->frame_width,
+	     tmp_win->title_height + tmp_win->boundary_width,
+	     False);
+
+  if (tmp_win->flags & ICONIFIED) {
+    DeIconify(tmp_win);
+  }
+    
+  XGetWindowAttributes(dpy, tmp_win->w, &winattrs);
+  eventMask = winattrs.your_event_mask;
+  
+  tmp_win->flags &= ~MAPPED;
+  XSelectInput(dpy, tmp_win->w, eventMask & ~StructureNotifyMask);
+  XUnmapWindow(dpy, tmp_win->w);
+  XSelectInput(dpy, tmp_win->w, eventMask);
+  
+  SetMapStateProp(tmp_win, IconicState);
+  
+  BroadcastPacket(M_WINDOWSHADE, 3,
+		  tmp_win->w, tmp_win->frame, (unsigned long)tmp_win);
+}
+
 void WindowShade(XEvent *eventp,Window w,FvwmWindow *tmp_win,
 		 unsigned long context, char *action, int *Module)
 {
-    int n = 0;
-
+    int val = 0;
+    
     if (DeferExecution(eventp,&w,&tmp_win,&context, SELECT,ButtonRelease))
 	return;
 
+    GetIntegerArguments(action, NULL, &val, 1);
+
     if (!(tmp_win->flags & TITLE) || (tmp_win->flags & MAXIMIZED)) {
-	XBell(dpy, 0);
-	return;
-    }
-    while (isspace(*action))++action;
-    if (isdigit(*action))
-	sscanf(action,"%d",&n);
-
-    if (((tmp_win->buttons & WSHADE)||(n==2))&&(n!=1))
-    {
-	tmp_win->buttons &= ~WSHADE;
-	SetupFrame(tmp_win,
-		   tmp_win->frame_x,
-		   tmp_win->frame_y,
-		   tmp_win->orig_wd,
-		   tmp_win->orig_ht,
-		   True);
-        BroadcastPacket(M_DEWINDOWSHADE, 3,
-                        tmp_win->w, tmp_win->frame, (unsigned long)tmp_win);
+      FunctionNotAllowed();
+      return;
     }
+
+    if (((val > 0) && (tmp_win->buttons & WSHADE)) ||
+        ((val < 0) && !(tmp_win->buttons & WSHADE)))
+      return;
+
+    if (tmp_win->buttons & WSHADE)
+      {
+	UnShade (tmp_win);
+      } 
     else
-    {
-	tmp_win->buttons |= WSHADE;
-	SetupFrame(tmp_win,
-		   tmp_win->frame_x,
-		   tmp_win->frame_y,
-		   tmp_win->frame_width,
-		   tmp_win->title_height + tmp_win->boundary_width,
-		   False);
-        BroadcastPacket(M_WINDOWSHADE, 3,
-                        tmp_win->w, tmp_win->frame, (unsigned long)tmp_win);
-    }
+      {
+	Shade (tmp_win);
+      }
 }
 #endif /* WINDOWSHADE */
 
--- events.c.last	Fri Jan 15 02:02:08 1999
+++ events.c	Fri Jan 15 01:58:50 1999
@@ -796,6 +796,11 @@
 
   HandleMapRequestKeepRaised(None);
 }
+
+#ifdef WINDOWSHADE
+void UnShade (FvwmWindow *);
+#endif
+
 void HandleMapRequestKeepRaised(Window KeepRaised)
 {
   extern long isIconicState;
@@ -848,6 +853,13 @@
 
   if(KeepRaised != None)
     XRaiseWindow(dpy,KeepRaised);
+#ifdef WINDOWSHADE
+  if (Tmp_win->buttons & WSHADE) 
+    {
+      UnShade (Tmp_win);
+    } 
+  else
+#endif
   /* If it's not merely iconified, and we have hints, use them. */
   if (!(Tmp_win->flags & ICONIFIED))
     {


