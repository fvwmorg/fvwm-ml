+void IconifyGroup(FvwmWindow *tmp_win, int def_x, int def_y)
+{
+  FvwmWindow *t, *group;
+
+  if (!tmp_win || 
+      tmp_win->tmpflags.GroupIconified ||
+      !tmp_win->wmhints ||
+      !(tmp_win->wmhints->flags & WindowGroupHint) ||
+      (tmp_win->wmhints->window_group == None) ||
+      (tmp_win->wmhints->window_group == Scr.Root)) {
+    return;
+  }
+
+  group = NULL;
+
+  for (t = Scr.FvwmRoot.next; t != NULL; t = t->next) {
+    if (t->w == tmp_win->wmhints->window_group) {
+      group = t;
+      fvwm_msg(DBG, "IconifyGroup", "group leader %x found\n", t->w);
+    } else if ((t->wmhints) &&
+               (t->wmhints->flags & WindowGroupHint)&&
+               (t->wmhints->window_group == tmp_win->wmhints->window_group)) {
+
+      t->tmpflags.GroupIconified = 1;
+      t->tmpflags.IndividuallyIconified = (t->flags & ICONIFIED) ? 1 : 0;
+      t->tmpflags.IconSuppressed = (t->flags & SUPPRESSICON) ? 1 : 0; 
+
+      fvwm_msg(DBG, "IconifyGroup", "%s group member %x found\n",
+	     (t->tmpflags.IndividuallyIconified ? "iconic" : "normal"),
+	     t->w); 
+ 
+      t->flags |= SUPPRESSICON;
+
+      Iconify(t, t->icon_x_loc, t->icon_y_loc); 
+    }    
+  }
+
+  if (group == NULL) {
+    /* The client has specified a group leader which he didn't
+       map. We'll assume that it is a window created specifically
+       for this purpose. We really shouldn't map it, but it is too 
+       complicated to create a FvwmWindow structure for it without 
+       mapping it. The FvwmWindow is really too much overhead only
+       for its icon, too bad. It will also have unwanted effects
+       in the FvwmWindow list structures, I guess. 
+       We can at least move it out of sight. */
+
+
+    /* Set window_group so that this window is recognizable as 
+       group leader */
+    XWMHints *wmh = XGetWMHints (dpy, tmp_win->wmhints->window_group);
+    
+    if (wmh == NULL)
+      wmh = XAllocWMHints ();
+    
+    if (!(wmh->flags & WindowGroupHint)) {
+      wmh->flags |= WindowGroupHint;
+      wmh->window_group = tmp_win->wmhints->window_group;
+      XSetWMHints (dpy, tmp_win->wmhints->window_group, wmh);
+    }
+    XFree (wmh);
+      
+    group = AddWindow (tmp_win->wmhints->window_group);
+    /* And we keep it out of sight */
+    group->flags |= WINDOWLISTSKIP|CirculateSkip;
+
+    /* These settings make sure that the group leader will not
+       show up on screen when you deiconify the group. It will
+       still show up if you deiconify only the group leader window.
+       But I have changed check_if_function_allowed to disable
+       iconify on windows with the OnlyGroupLeader flag set...
+    */
+    group->flags |= SUPPRESSICON;
+    group->flags |= ICONIFIED;
+    group->tmpflags.OnlyGroupLeader = 1; 
+
+    fvwm_msg(DBG, "IconifyGroup", "artificial group leader %x created\n", 
+	     group->w);
+  }
+  
+  group->tmpflags.GroupIconified = 1;
+  group->tmpflags.IndividuallyIconified = (group->flags & ICONIFIED) ? 1 : 0;
+  group->tmpflags.IconSuppressed = (group->flags & SUPPRESSICON) ? 1 : 0; 
+  
+  group->flags &= ~SUPPRESSICON;
+  
+  Iconify(group, def_x, def_y); 
+      
+}
+
+void DeIconifyGroup(FvwmWindow *tmp_win)
+{
+  FvwmWindow *t;
+
+  if (!tmp_win || 
+      !tmp_win->tmpflags.GroupIconified ||
+      !tmp_win->wmhints ||
+      !(tmp_win->wmhints->flags & WindowGroupHint) ||
+      (tmp_win->wmhints->window_group == None) ||
+      (tmp_win->wmhints->window_group == Scr.Root)) {
+    return;
+  }
+
+  for (t = Scr.FvwmRoot.next; t != NULL; t = t->next) {
+    if ((t->w == tmp_win->wmhints->window_group) ||
+	((t->wmhints) &&
+	 (t->wmhints->flags & WindowGroupHint) &&
+	 (t->wmhints->window_group == tmp_win->wmhints->window_group))) {
+      t->tmpflags.GroupIconified = 0;
+      if (t->tmpflags.IconSuppressed) {
+        t->flags |= SUPPRESSICON;
+      } else {
+        t->flags &= ~SUPPRESSICON;
+      }
+      if (t->tmpflags.IndividuallyIconified) {
+        fvwm_msg(DBG, "DeIconifyGroup", "restore group %s %x to iconic", 
+	         (t->w == tmp_win->wmhints->window_group) ? "leader" : "member",
+	         t->w);       
+        Iconify (t, t->icon_x_loc, t->icon_y_loc);
+      } else {
+        fvwm_msg(DBG, "DeIconifyGroup", "restore group %s %x to normal", 
+	         (t->w == tmp_win->wmhints->window_group) ? "leader" : "member",
+	         t->w);       
+        DeIconify (t);
+      }
+    }    
+  }
 }
