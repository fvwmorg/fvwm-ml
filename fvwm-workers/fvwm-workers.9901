+
+void WindowMenu(XEvent *Event)
+{
+  char command[100];
+  PagerWindow *t;
+
+  if (MenuName == NULL) 
+    return;
+
+  sprintf(command, "Popup %s", MenuName);
+
+  t = Start;
+  while ((t != NULL)&&(t->PagerView != Event->xbutton.subwindow))
+    t= t->next;
+
+  if(t==NULL)
+    {
+      t = Start;
+      while ((t != NULL)&&(t->IconView != Event->xbutton.subwindow))
+	t= t->next;
+      if(t!=NULL)
+	{
+          XUngrabPointer(dpy, CurrentTime);
+          XSync(dpy, 0);
+          SendInfo(fd, command, t->icon_w);
+	  
+	  return;
+	}
+    }
+
+  if(t == NULL)
+    return;
+
+  XUngrabPointer(dpy, CurrentTime);
+  XSync(dpy, 0);
+  SendInfo(fd, command, t->w);
 }
 
