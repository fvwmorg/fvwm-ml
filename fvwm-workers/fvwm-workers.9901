-      if(strncasecmp(text,"popup",5)==0) 
+      if(strncasecmp(text,"popup",5)==0) {
+          ButtonWindow = tmp_win;
 	  Event.xany.type = ButtonPress;
-      else
+      } else
 	  Event.xany.type = ButtonRelease;
       Event.xany.window = w;
 
