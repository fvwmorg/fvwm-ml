>   
> while(1) {
>   XUnmapWindow(dpy, win);
>   /* Wait for window to become unmapped */
>   do {
>     XNextEvent(dpy, &ev);
>   } while(ev.type != UnmapNotify);
> 
>   XMapWindow(dpy, win);
>   /* wait for the window to become mapped */
>   do {
>     XNextEvent(dpy, &ev);
>   } while(ev.type != MapNotify);
> }
> 
> 
