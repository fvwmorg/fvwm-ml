#4  0x08099fdf in execute_complex_function (cond_rc=0xbffff0ac, exc=0x81e5d98,
    action=0x81e9308 "M", desperate=0x5) at functions.c:957
#5  0x0809993a in __execute_function (cond_rc=0xbffff0ac, exc=0x81e9240,
