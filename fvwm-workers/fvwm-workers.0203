+++ fvwm/virtual.c	2002/03/15 13:52:19
@@ -991,11 +991,11 @@
   }
   Scr.Vx = newx;
   Scr.Vy = newy;
-  BroadcastPacket(
-    M_NEW_PAGE, 5, Scr.Vx, Scr.Vy, Scr.CurrentDesk, Scr.VxMax, Scr.VyMax);
 
   if (deltax || deltay)
   {
+    BroadcastPacket(
+      M_NEW_PAGE, 5, Scr.Vx, Scr.Vy, Scr.CurrentDesk, Scr.VxMax, Scr.VyMax);
 
