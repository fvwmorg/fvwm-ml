+ /* Allow user to specify particular mappings for hilite/shadow colors */
+  
+ typedef struct ColorHiliteMap {
+   struct ColorHiliteMap *next;
+   XColor normal;
+   XColor hilite;
+   XColor shadow;
+ } ColorHiliteMap;
+  
+ static ColorHiliteMap *hilite_maps;
+ 
+ 
+ void
+ AddHiliteShadowMapping(char *normal, char *hilite, char *shadow)
+ {
+   extern Colormap PictureCMap;
+   extern Display *PictureSaveDisplay;
+   ColorHiliteMap *himap = (ColorHiliteMap *)safemalloc(sizeof(ColorHiliteMap));
+   if (!himap)
+     goto dead;
+   
+   if (!XParseColor (PictureSaveDisplay, PictureCMap, normal, &himap->normal)) {
+     fprintf (stderr, "[AddHiliteMap]: can't parse color `%s'\n", normal);
+     goto dead;
+   }
+   /* Since a color's definition may change when it is allocated, we allocate
+      it here and re-query it to get the REAL red,green,blue components */
+   if (XAllocColor (PictureSaveDisplay, PictureCMap, &himap->normal)) {
+     XQueryColor (PictureSaveDisplay, PictureCMap, &himap->normal);
+   }
+   
+   if (!XParseColor (PictureSaveDisplay, PictureCMap, hilite, &himap->hilite)) {
+     fprintf (stderr, "[AddHiliteMap]: can't parse color `%s'\n", hilite);
+     goto dead;
+   }
+     
+   if (!XParseColor (PictureSaveDisplay, PictureCMap, shadow, &himap->shadow)) {
+     fprintf (stderr, "[AddHiliteMap]: can't parse color `%s'\n", shadow);
+     goto dead;
+   }
+ 
+   himap->next = hilite_maps;
+   hilite_maps = himap;
+   return;
+   
+  dead:
+   if (himap)
+     free(himap);
+ }
+ 
+ 
  #define DARKNESS_FACTOR 0.5
+ #define BRIGHTNESS_FACTOR 1.4
+ 
+ static Pixel
+ get_shadow_or_hilite(Pixel background, int is_hilite)
+ {
+   extern Colormap PictureCMap;
+   extern Display *PictureSaveDisplay;
+   ColorHiliteMap *cbm;
+   XColor c;
+   
+   c.pixel = background;
+   XQueryColor (PictureSaveDisplay, PictureCMap, &c);
+   
+   /* check existing shadow/hilite mappings */
+   for (cbm = hilite_maps; cbm; cbm = cbm->next)
+     if (c.red == cbm->normal.red
+ 	&& c.green == cbm->normal.green
+ 	&& c.blue == cbm->normal.blue) {
+       XColor *answer = (is_hilite ? &cbm->hilite : &cbm->shadow);
+       XAllocColor (PictureSaveDisplay, PictureCMap, answer);
+       return answer->pixel;
+     }
+   
+   /* if not found, use the default color multiplication */
+   color_mult(&c.red, &c.green, &c.blue,
+ 	     (is_hilite ? BRIGHTNESS_FACTOR : DARKNESS_FACTOR));
+   XAllocColor (PictureSaveDisplay, PictureCMap, &c);
+   return c.pixel;
+ }
+ 
  Pixel GetShadow(Pixel background) {
!   return get_shadow_or_hilite(background, 0);
  }
  
  Pixel GetHilite(Pixel background) {
!   return get_shadow_or_hilite(background, 1);
  }
