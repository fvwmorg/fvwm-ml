+char* CreateColorString(Pixel pixel) {
+    char tmp[128];
+    XColor color;
+    color.pixel=pixel;
+    color.red=0;
+    color.green=0;
+    color.blue=0;
+    XQueryColor(dpy, Pcmap, &color);
+    sprintf(tmp, "#%02x%02x%02x", (color.red>>8)&0xff, (color.green>>8)&0xff, (color.blue>>8)&0xff);
+    return safestrdup(tmp);
+}
 
