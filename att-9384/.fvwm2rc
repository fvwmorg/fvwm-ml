+			"I" Exec coolmail -f /var/spool/mail/root -p /root/mail.wav -o /dev/dsp8 -geometry 65x65+0+95 -e "xterm -title Mutt:Root -geometry 80x89 -sb -sl 500 -j -ls -bg black -fg yellow -fn 6X13 -e /home/alaric/bin/mail.root"
