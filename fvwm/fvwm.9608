    print 'IconPath   ==', info.get_iconpath()
    print 'PixmapPath ==', info.get_pixmappath()
    print 'ClickTime  ==', info.get_clicktime()
    print '1 =========='
    printlist(info.get_infolines())
    print '2 =========='
    printlist(info.get_infolines('FvwmIdent'))
