      gcv.font = balloon.font->fid;
  
      BalloonGC = XCreateGC(dpy, balloon.w,
!                           GCFont,
                            &gcv);
    } /* ShowBalloons */
  }
--- 444,457 ----
                                CWOverrideRedirect | CWEventMask | CWBackPixel,
