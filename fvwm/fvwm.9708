@@ -1060,6 +1065,36 @@
 			colors = GetNextToken (colors, &WindowHiBack);
 		}
 	}
+
+      
+      /* ... and get Balloon config options ...
+         -- ric@giccs.georgetown.edu */
+      else if ( (strlen(&tline[0]) > 1) &&
+                (strncasecmp (tline, CatString3 ("*", MyName, "Balloons"),
+                              Clength + 9) == 0) ) {
+        ShowBalloons = 1;
