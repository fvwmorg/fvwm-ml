        while((l=strlen(s))<sizeof(s) && s[l-1]=='\n' && s[l-2]=='\\')
  	fgets(s+l-2,sizeof(s)-l,f);
        /* And comments: */
        t=s;
!       while(*t)
! 	{
! 	  if(*t=='#' && (t==s || *(t-1)!='\\'))
! 	    {
! 	      *t=0;
! 	      break;
! 	    }
! 	  t++;
! 	}
!       t=s;trimleft(t);
        if(*t)ParseConfigLine(&ub,t);
      }
  
--- 784,795 ----
        while((l=strlen(s))<sizeof(s) && s[l-1]=='\n' && s[l-2]=='\\')
  	fgets(s+l-2,sizeof(s)-l,f);
        /* And comments: */
+ 
+ 
        t=s;
!       trimleft(t);
!       if (t[0]=='#')*t=0;
! 
        if(*t)ParseConfigLine(&ub,t);
      }
  
