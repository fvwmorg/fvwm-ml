+      else if ( (strlen(&tline[0]) > 1) &&
+                (strncasecmp (tline, CatString3 ("*", MyName, "BalloonFont"), 
+                              Clength + 12) == 0) ) {
+        CopyString(&BalloonFont, &tline[Clength+12]);
