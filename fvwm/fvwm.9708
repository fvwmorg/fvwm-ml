+      else if ( (strlen(&tline[0]) > 1) &&
+                (strncasecmp (tline, CatString3 ("*", MyName, "BalloonBack"),
+                              Clength + 12) == 0) ) {
+        if (Scr.d_depth > 1)
+          CopyString(&BalloonBack, &tline[Clength+12]);
+      }
+
+      else if ( (strlen(&tline[0]) > 1) &&
+                (strncasecmp (tline, CatString3 ("*", MyName, "BalloonFore"), 
+                              Clength + 12) == 0) ) {
+        if (Scr.d_depth > 1)
+          CopyString(&BalloonFore, &tline[Clength+12]);
+      }
