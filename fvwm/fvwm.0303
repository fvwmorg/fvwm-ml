+               /* Get the window ID: */
+               char winId[20];
+               sprintf(winId, "0x%x", (unsigned int)FW_W(t));
+ 
                if (matchWildcards(name, t->class.res_class) == TRUE ||
                    matchWildcards(name, t->class.res_name) == TRUE ||
