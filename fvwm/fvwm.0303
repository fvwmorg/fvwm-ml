Content-Disposition: attachment; filename=fvwm-mousecmd-convert.awk
Content-Type: text/plain; name=fvwm-mousecmd-convert.awk; charset=UTF-8
Content-Transfer-Encoding: quoted-printable

#!/usr/bin/awk -f

function behavior_changed(command)
{
	if (command =3D=3D "animatedmove" ||
		command =3D=3D "move" ||
		command =3D=3D "movetopage" ||
		command =3D=3D "movetoscreen" ||
		command =3D=3D "resize" ||
		command =3D=3D "resizemaximize" ||
		command =3D=3D "resizemove" ||
		command =3D=3D "resizemovemaximize")
			return 1;
	return 0;
}

{
	if (tolower($1) =3D=3D "mouse" && behavior_changed(tolower($5))) {
		buildfunction[tolower($5)] =3D 1;
		$5 =3D "Converted_Interactive"tolower($5);
	}
	print $0;
}

END {
	for (current in buildfunction) {
		print "\nDestroyFunction Converted_Interactive"current
		print "AddToFunc Converted_Interactive"current
		print "+ \"I\" "current" $*"
	}
}

--=-GSE+DvEHbkB68EbUnrg9--

