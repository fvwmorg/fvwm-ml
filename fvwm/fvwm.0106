monstro> find /usr/lpp/X11/lib -name 'libXext*' -print
/usr/lpp/X11/lib/R6/libXext.a
/usr/lpp/X11/lib/R6/inst_updt/libXext.a
/usr/lpp/X11/lib/R5/libXext.a

