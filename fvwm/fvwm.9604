+#if defined(i386BsdArchitecture) && defined(i386Bsdi)
+        C++FLAGS = $(CXXFLAGS)
+#endif
 ProgramTargetName(program): $(objs) $(deplib)				@@\
 	RemoveTargetProgram($@)						@@\
 	CplusplusLinkRule($@,$(CXXLDOPTIONS),$(objs),locallib $(LDLIBS) syslib) @@\
