 #include "../../fvwm/sun_headers.h"
 #endif
 
+#ifdef HAVE_WAITPID
+#define ReapChildrenPid(pid) waitpid(pid, NULL, WNOHANG)
+#else
+#define ReapChildrenPid(pid) wait4(pid, NULL, WNOHANG, NULL)
+#endif
+
+#ifdef sony_news
+typedef int pid_t;
+#define setvbuf(a,b,c,d) setlinebuf(a)
