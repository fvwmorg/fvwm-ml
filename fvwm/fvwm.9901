  al> configure:4525: gcc -o conftest -g -O2  -I/opt/fvwm/include
  al> -I/usr/openwin/include  conftest.c  -L/opt/fvwm/lib -lXpm
  al> -L/usr/openwin/lib -R/usr/openwin/lib  -
  al> lX11 -lsocket  -lnsl 1>&5
